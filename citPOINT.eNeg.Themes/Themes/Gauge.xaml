<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:telerik="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls"
					xmlns:gauge="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.Gauge"
					xmlns:gauges="clr-namespace:Telerik.Windows.Controls.Gauges;assembly=Telerik.Windows.Controls.Gauge">

	<?BODY BEGIN?>
	<!-- Default scale bar apperance -->
	<SolidColorBrush x:Key="ScaleBarForeground" Color="White" />
	<SolidColorBrush x:Key="ScaleBarBackground" Color="Transparent" />
	<SolidColorBrush x:Key="ScaleBarBorderBrush" Color="Beige" />

	<Style TargetType="gauges:RangeList">
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="gauges:RangeList">
					<ItemsPresenter />
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="ItemsPanel">
			<Setter.Value>
				<ItemsPanelTemplate>
					<Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
				</ItemsPanelTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<!-- Default range appearance -->
	<SolidColorBrush x:Key="RangeBackground" Color="Transparent" />
	<SolidColorBrush x:Key="RangeBorderBrush" Color="CadetBlue" />

	<Style TargetType="gauges:RadialRange">
		<Setter Property="StartWidth" Value="0.01" />
		<Setter Property="EndWidth" Value="0.1" />
		<Setter Property="Location" Value="Inside" />
		<Setter Property="Background" Value="White" />
		<Setter Property="BorderBrush" Value="{StaticResource RangeBorderBrush}" />
		<Setter Property="StrokeThickness" Value="1" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="gauges:RadialRange">
					<Path x:Name="PART_RangePath" 
			              Fill="{TemplateBinding Background}" 
			              Stroke="{TemplateBinding BorderBrush}"
			              StrokeThickness="{TemplateBinding StrokeThickness}"/>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style TargetType="gauges:LinearRange">
		<Setter Property="StartWidth" Value="0.01" />
		<Setter Property="EndWidth" Value="0.1" />
		<Setter Property="Location" Value="Inside" />
		<Setter Property="Background" Value="White" />
		<Setter Property="BorderBrush" Value="{StaticResource RangeBorderBrush}" />
		<Setter Property="StrokeThickness" Value="1" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="gauges:LinearRange">
					<Path x:Name="PART_RangePath" 
			              Fill="{TemplateBinding Background}" 
			              Stroke="{TemplateBinding BorderBrush}"
			              StrokeThickness="{TemplateBinding StrokeThickness}"/>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<!-- Needle Pin Point -->
	<LinearGradientBrush x:Key="NeedlePinPointBottom" 
						 StartPoint="0.5,0" 
						 EndPoint="0.5,1" 
						 MappingMode="RelativeToBoundingBox">
		<GradientStop Color="#FFFEFFFE" Offset="0"/>
		<GradientStop Color="#FFFEFFFE" Offset="0.505493"/>
		<GradientStop Color="#FFD8DADB" Offset="0.527466"/>
		<GradientStop Color="#FFB2B5B7" Offset="1"/>
	</LinearGradientBrush>

	<RadialGradientBrush x:Key="NeedlePinPointTop" 
						 RadiusX="0.499858" 
						 RadiusY="0.499837" 
						 Center="0.499939,0.499956" 
						 GradientOrigin="0.499939,0.499956">
		<RadialGradientBrush.RelativeTransform>
			<TransformGroup>
				<RotateTransform CenterX="0.499939" CenterY="0.499956" Angle="68.2337"/>
			</TransformGroup>
		</RadialGradientBrush.RelativeTransform>
		<GradientStop Color="#CDFF1F00" Offset="0"/>
		<GradientStop Color="#CD900C00" Offset="1"/>
	</RadialGradientBrush>

	<!-- Needle -->
	<LinearGradientBrush x:Key="NeedleArrowBrush" StartPoint="0.767139,-0.0611001" EndPoint="0.767139,1.0387">
		<GradientStop Color="#FFFEFFFE" Offset="0"/>
		<GradientStop Color="#FFFEFFFE" Offset="0.505493"/>
		<GradientStop Color="#FFD8DADB" Offset="0.527466"/>
		<GradientStop Color="#FFB2B5B7" Offset="1"/>
	</LinearGradientBrush>

	<Style TargetType="gauges:Needle">
		<Setter Property="Location" Value="OverCenter" />
		<Setter Property="RelativeHeight" Value="0.2" />
		<Setter Property="RelativeShift" Value="0.8" />
		<Setter Property="Background" Value="{StaticResource NeedleArrowBrush}" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="gauges:Needle">
					<Grid x:Name="PART_Grid" 
						  HorizontalAlignment="Stretch" 
						  VerticalAlignment="Stretch">
						<Grid.ColumnDefinitions>
							<ColumnDefinition Width="*"/>
							<!-- Column 0 - tail -->
							<ColumnDefinition Width="*"/>
							<!-- Column 1 - pin point -->
							<ColumnDefinition Width="*"/>
							<!-- Column 2 - arrow -->
						</Grid.ColumnDefinitions>
						<Grid.RowDefinitions>
							<RowDefinition Height="*"/>
						</Grid.RowDefinitions>

						<Grid x:Name="PART_Shape" 
							  Grid.ColumnSpan="3" 
							  Margin="0">
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="*" />
							</Grid.ColumnDefinitions>
							<Grid.RowDefinitions>
								<RowDefinition Height="0.3*" />
								<RowDefinition Height="0.4*" />
								<RowDefinition Height="0.3*" />
							</Grid.RowDefinitions>

							<Path Grid.Row="1"
								  Grid.Column="0"
								  Stretch="Fill" 
								  Data="F1 M 0.169559,0.0163666L 0.223558,0.00818365L 0.169559,6.10352e-008L 0.158926,0.00496332L 0.00148178,0.00770581L 6.33129e-009,0.00818304L 0.00148113,0.00866058L 0.158925,0.0114028L 0.169559,0.0163666 Z " 
								  Fill="{TemplateBinding Background}"
								  Stroke="{TemplateBinding BorderBrush}"
								  StrokeThickness="1">
							</Path>
						</Grid>

						<Grid Grid.Column="1">
							<Grid.RowDefinitions>
								<RowDefinition Height="0.125*" />
								<RowDefinition Height="0.75*" />
								<RowDefinition Height="0.125*" />
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="0.125*" />
								<ColumnDefinition Width="0.75*" />
								<ColumnDefinition Width="0.125*" />
							</Grid.ColumnDefinitions>
							<Ellipse Grid.Column="1"
			                         Grid.Row="1"
			                         Stretch="Uniform"
									 Fill="{StaticResource NeedlePinPointBottom}">
							</Ellipse>
						</Grid>

						<Grid Grid.Column="1">
							<Grid.RowDefinitions>
								<RowDefinition Height="0.25*" />
								<RowDefinition Height="0.5*" />
								<RowDefinition Height="0.25*" />
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="0.25*" />
								<ColumnDefinition Width="0.5*" />
								<ColumnDefinition Width="0.25*" />
							</Grid.ColumnDefinitions>
							<Ellipse Grid.Column="1"
			                         Grid.Row="1"
			                         Stretch="Uniform"
									 Fill="{StaticResource NeedlePinPointTop}">
							</Ellipse>
						</Grid>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<!-- Default state indicator appearance -->
	<RadialGradientBrush x:Key="StateIndicatorBackground">
		<GradientStop Offset="0" Color="Yellow" />
		<GradientStop Offset="0.6" Color="Orange" />
		<GradientStop Offset="1" Color="OrangeRed" />
	</RadialGradientBrush>

	<Style TargetType="gauges:StateIndicator">
		<Setter Property="Top" Value="0.25" />
		<Setter Property="Left" Value="0.45" />
		<Setter Property="RelativeWidth" Value="0.2" />
		<Setter Property="RelativeHeight" Value="0.2" />
		<Setter Property="IsAnimated" Value="True" />
		<Setter Property="Background" Value="{StaticResource StateIndicatorBackground}" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="gauges:StateIndicator">
					<Border BorderBrush="Transparent" BorderThickness="2">
						<Grid VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
							<Ellipse Stretch="Fill" 
								 Fill="{TemplateBinding Background}" 
								 Stroke="{TemplateBinding BorderBrush}"
								 StrokeThickness="{TemplateBinding StrokeThickness}" />
						</Grid>
					</Border>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<!-- Default tick background -->
	<SolidColorBrush x:Key="DefaultTickBackground" Color="White" />

	<!-- Default custom tick mark background -->
	<SolidColorBrush x:Key="DefaultCustomTickBackground" Color="Red" />

	<Style TargetType="gauges:CustomTickMark">
		<Setter Property="Location" Value="OverCenter" />
		<Setter Property="Offset" Value="0" />
		<Setter Property="Format" Value="{}{0:F0}" />
		<Setter Property="Length" Value="0.1" />
		<Setter Property="TickWidth" Value="1" />
		<Setter Property="Background" Value="{StaticResource DefaultCustomTickBackground}" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="gauges:CustomTickMark">
					<Grid>
						<Polygon Points="0,5 5,0 10,5 5,10"
								 Stretch="Fill" 
								 HorizontalAlignment="Stretch"
								 VerticalAlignment="Stretch"
								 Fill="{TemplateBinding Background}" 
								 Stroke="{TemplateBinding BorderBrush}" 
								 StrokeThickness="{TemplateBinding BorderThickness}" />
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="ItemTemplate">
			<Setter.Value>
				<DataTemplate>
					<TextBlock HorizontalAlignment="Center"
							   FontFamily="{Binding Path=Properties.FontFamily}"
							   FontSize="{Binding Path=Properties.FontSize}"
							   Foreground="{Binding Path=Properties.Foreground}"
							   Margin="4,2,4,2">
						<TextBlock.Text>
							<Binding>
								<Binding.Converter>
									<gauges:LabelFormatConverter />
								</Binding.Converter>
							</Binding>
						</TextBlock.Text>
					</TextBlock>
				</DataTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style TargetType="gauges:TickList">
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="gauges:TickList">
					<ItemsPresenter />
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="ItemsPanel">
			<Setter.Value>
				<ItemsPanelTemplate>
					<Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
				</ItemsPanelTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="ItemTemplateSelector">
			<Setter.Value>
				<gauges:TickTemplateSelector />
			</Setter.Value>
		</Setter>
	</Style>

	<Style TargetType="gauges:TickProperties">
		<Setter Property="Location" Value="OverCenter" />
		<Setter Property="Offset" Value="0" />
		<Setter Property="Length" Value="0.1" />
		<Setter Property="TickWidth" Value="0.1" />
		<Setter Property="Background" Value="{StaticResource DefaultTickBackground}" />
		<Setter Property="ItemTemplate">
			<Setter.Value>
				<DataTemplate>
					<Rectangle Fill="{Binding Path=Properties.Background}" 
							   Stroke="{Binding Path=Properties.BorderBrush}"
							   StrokeThickness="{Binding Path=Properties.BorderThickness.Left}"
							   Stretch="Fill" />
				</DataTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style x:Key="TriangleTickStyle" TargetType="gauges:TickProperties">
		<Setter Property="Location" Value="OverOutside" />
		<Setter Property="Offset" Value="0" />
		<Setter Property="Length" Value="0.07" />
		<Setter Property="TickWidth" Value="0.7" />
		<Setter Property="Background" Value="{StaticResource DefaultTickBackground}" />
		<Setter Property="ItemTemplate">
			<Setter.Value>
				<DataTemplate>
					<Polygon Points="0.5,0 1,1 0,1"
							 Stretch="Fill"
							 Fill="{Binding Path=Properties.Background}" 
							 Stroke="{Binding Path=Properties.BorderBrush}"
							 StrokeThickness="{Binding Path=Properties.BorderThickness.Left}" />
				</DataTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style TargetType="gauges:Tick">
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="gauges:Tick">
					<Grid>
						<ContentPresenter Content="{TemplateBinding Content}"
										  HorizontalAlignment="Stretch"
										  VerticalAlignment="Stretch" />
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style TargetType="gauges:LabelList">
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="gauges:LabelList">
					<ItemsPresenter />
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="ItemsPanel">
			<Setter.Value>
				<ItemsPanelTemplate>
					<Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
				</ItemsPanelTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style TargetType="gauges:TickLabel">
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="gauges:TickLabel">
					<ContentPresenter Content="{TemplateBinding Content}" />
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style TargetType="gauges:LabelProperties">
		<Setter Property="Location" Value="Outside" />
		<Setter Property="FontFamily" Value="Segoe UI" />
		<Setter Property="FontSize" Value="12" />
		<Setter Property="Foreground" Value="White" />
		<Setter Property="Offset" Value="0" />
		<Setter Property="Format" Value="{}{0:F0}" />
		<Setter Property="ItemTemplate">
			<Setter.Value>
				<DataTemplate>
					<TextBlock HorizontalAlignment="Center"
							   FontFamily="{Binding Path=Properties.FontFamily}"
							   FontSize="{Binding Path=Properties.FontSize}"
							   Foreground="{Binding Path=Properties.Foreground}"
							   Margin="4,2,4,2">
						<TextBlock.Text>
							<Binding>
								<Binding.Converter>
									<gauges:LabelFormatConverter />
								</Binding.Converter>
							</Binding>
						</TextBlock.Text>
					</TextBlock>
				</DataTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<!-- Default bar-indicator appearance -->
	<SolidColorBrush x:Key="BarBackground" Color="#FFFFFFFF" />
	<SolidColorBrush x:Key="BarBorderBrush" Color="Blue" />

	<Style TargetType="gauges:LinearBar">
		<Setter Property="Offset" Value="0" />
		<Setter Property="Location" Value="OverCenter" />
		<Setter Property="StartWidth" Value="0.025" />
		<Setter Property="EndWidth" Value="0.025" />
		<Setter Property="Background" Value="{StaticResource BarBackground}" />
		<Setter Property="BorderBrush" Value="{StaticResource BarBorderBrush}" />
		<Setter Property="StrokeThickness" Value="0" />
		<Setter Property="IsAnimated" Value="True" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="gauges:LinearBar">
					<Grid>
						<Path x:Name="PART_BarPath" 
							  Fill="{TemplateBinding Background}" 
							  Stroke="{TemplateBinding BorderBrush}"
							  StrokeThickness="{TemplateBinding StrokeThickness}"/>
						<Path x:Name="PART_EmptyPath" 
							  Fill="{TemplateBinding EmptyFill}" 
							  Stroke="{TemplateBinding BorderBrush}"
							  StrokeThickness="{TemplateBinding StrokeThickness}"/>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style TargetType="gauges:RadialBar">
		<Setter Property="Offset" Value="0" />
		<Setter Property="Location" Value="OverCenter" />
		<Setter Property="StartWidth" Value="0.1" />
		<Setter Property="EndWidth" Value="0.1" />
		<Setter Property="Background" Value="{StaticResource BarBackground}" />
		<Setter Property="BorderBrush" Value="{StaticResource BarBorderBrush}" />
		<Setter Property="StrokeThickness" Value="0" />
		<Setter Property="IsAnimated" Value="True" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="gauges:RadialBar">
					<Grid>
						<Path x:Name="PART_BarPath" 
							  Fill="{TemplateBinding Background}" 
							  Stroke="{TemplateBinding BorderBrush}"
							  StrokeThickness="{TemplateBinding StrokeThickness}"/>
						<Path x:Name="PART_EmptyPath" 
							  Fill="{TemplateBinding EmptyFill}" 
							  Stroke="{TemplateBinding BorderBrush}"
							  StrokeThickness="{TemplateBinding StrokeThickness}"/>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<!-- Marker -->
	<LinearGradientBrush x:Key="MarkerBackground" StartPoint="0.767139,-0.0611001" EndPoint="0.767139,1.0387">
		<GradientStop Color="#FFFEFFFE" Offset="0"/>
		<GradientStop Color="#FFFEFFFE" Offset="0.505493"/>
		<GradientStop Color="#FFD8DADB" Offset="0.527466"/>
		<GradientStop Color="#FFB2B5B7" Offset="1"/>
	</LinearGradientBrush>

	<Style TargetType="gauges:Marker">
		<Setter Property="Location" Value="Inside" />
		<Setter Property="Offset" Value="0" />
		<Setter Property="RelativeHeight" Value="0.1" />
		<Setter Property="RelativeWidth" Value="0.1" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="BorderBrush" Value="White" />
		<Setter Property="Background" Value="{StaticResource MarkerBackground}" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="gauges:Marker">
					<Grid HorizontalAlignment="Stretch" 
						  VerticalAlignment="Stretch">
						<Polygon Points="0.5,0 1,0.5 0.5,1 0,0.5"
								 Stretch="Fill"
								 Fill="{TemplateBinding Background}"
								 Stroke="{TemplateBinding BorderBrush}"
								 StrokeThickness="{TemplateBinding BorderThickness}"/>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style TargetType="gauges:IndicatorList">
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="gauges:IndicatorList">
					<ItemsPresenter />
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="ItemsPanel">
			<Setter.Value>
				<ItemsPanelTemplate>
					<Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
				</ItemsPanelTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style TargetType="gauges:RadialScale">
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="gauges:RadialScale">
					<Grid x:Name="PART_ScaleGrid">
						<ToolTipService.ToolTip>
							<ToolTip Content="" IsEnabled="False" Visibility="Collapsed"/>
						</ToolTipService.ToolTip>
						<Path x:Name="PART_ScalePath" 
								  Stroke="{TemplateBinding BorderBrush}"
								  StrokeThickness="{TemplateBinding StrokeThickness}"
								  Fill="{TemplateBinding Background}"
								  VerticalAlignment="Stretch"
								  HorizontalAlignment="Stretch" />
						<ItemsPresenter />
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="ItemsPanel">
			<Setter.Value>
				<ItemsPanelTemplate>
					<Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
				</ItemsPanelTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="Center" Value="0.5, 0.5" />
		<Setter Property="Radius" Value="0.87" />
		<Setter Property="StartAngle" Value="120" />
		<Setter Property="SweepAngle" Value="300" />
		<Setter Property="StartWidth" Value="0.1" />
		<Setter Property="EndWidth" Value="0.1" />
		<Setter Property="Foreground" Value="{StaticResource ScaleBarForeground}" />
		<Setter Property="Background" Value="{StaticResource ScaleBarBackground}" />
		<Setter Property="BorderBrush" Value="{StaticResource ScaleBarBorderBrush}" />
		<Setter Property="StrokeThickness" Value="1" />
		<Setter Property="LogarithmicBase" Value="10" />
		<Setter Property="Min" Value="0" />
		<Setter Property="Max" Value="100" />
		<Setter Property="MajorTicks" Value="10" />
		<Setter Property="MiddleTicks" Value="2" />
		<Setter Property="MinorTicks" Value="2" />
		<Setter Property="IsLabelRotated" Value="True" />
		<Setter Property="Multiplier" Value="1" />
	</Style>

	<Style TargetType="gauges:LinearScale">
		<Setter Property="Left" Value="0.5" />
		<Setter Property="Top" Value="0.1" />
		<Setter Property="RelativeHeight" Value="0.8" />
		<Setter Property="StartWidth" Value="0.1" />
		<Setter Property="EndWidth" Value="0.1" />
		<Setter Property="Foreground" Value="{StaticResource ScaleBarForeground}" />
		<Setter Property="Background" Value="{StaticResource ScaleBarBackground}" />
		<Setter Property="BorderBrush" Value="{StaticResource ScaleBarBorderBrush}" />
		<Setter Property="StrokeThickness" Value="1" />
		<Setter Property="LogarithmicBase" Value="10" />
		<Setter Property="Min" Value="0" />
		<Setter Property="Max" Value="100" />
		<Setter Property="MajorTicks" Value="4" />
		<Setter Property="MiddleTicks" Value="2" />
		<Setter Property="MinorTicks" Value="3" />
		<Setter Property="Multiplier" Value="1" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="gauges:LinearScale">
					<Grid>
						<ToolTipService.ToolTip>
							<ToolTip Content="" IsEnabled="False" Visibility="Collapsed"/>
						</ToolTipService.ToolTip>
						<Path x:Name="PART_ScalePath" 
								  Stroke="{TemplateBinding BorderBrush}"
								  StrokeThickness="{TemplateBinding StrokeThickness}"
								  Fill="{TemplateBinding Background}"
								  VerticalAlignment="Stretch"
								  HorizontalAlignment="Stretch" />
						<ItemsPresenter />
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="ItemsPanel">
			<Setter.Value>
				<ItemsPanelTemplate>
					<Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
				</ItemsPanelTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style TargetType="gauges:NumericScale">
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="Multiplier" Value="1" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="gauges:NumericScale">
					<Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
						<ToolTipService.ToolTip>
							<ToolTip Content="" IsEnabled="False" Visibility="Collapsed"/>
						</ToolTipService.ToolTip>
						<ItemsPresenter />
					</Canvas>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="ItemsPanel">
			<Setter.Value>
				<ItemsPanelTemplate>
					<Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
				</ItemsPanelTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style TargetType="gauges:NumericIndicator">
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="gauges:NumericIndicator">
					<Border Background="{TemplateBinding Background}"                            
							BorderBrush="{TemplateBinding BorderBrush}"
							BorderThickness="{TemplateBinding BorderThickness}">
						<Grid>
							<ItemsPresenter />
						</Grid>
					</Border>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="ItemsPanel">
			<Setter.Value>
				<ItemsPanelTemplate>
					<StackPanel Orientation="Horizontal" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
				</ItemsPanelTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style TargetType="gauges:NumberPosition">
		<Setter Property="Foreground" Value="White" />
		<Setter Property="FontFamily" Value="Arial" />
		<Setter Property="FontSize" Value="11" />
		<Setter Property="VerticalAlignment" Value="Stretch" />
		<Setter Property="HorizontalAlignment" Value="Stretch"/>
		<Setter Property="Padding" Value="1,0,1,0" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="gauges:NumberPosition">
					<Border BorderBrush="{TemplateBinding BorderBrush}"
							BorderThickness="{TemplateBinding BorderThickness}"
							Background="{TemplateBinding Background}">
						<Grid Margin="1,1,1,1">
							<ContentPresenter VerticalAlignment="{TemplateBinding VerticalAlignment}"
											  HorizontalAlignment="{TemplateBinding HorizontalAlignment}"/>
						</Grid>
					</Border>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style TargetType="gauge:RadGauge">
		<Setter Property="Template">
