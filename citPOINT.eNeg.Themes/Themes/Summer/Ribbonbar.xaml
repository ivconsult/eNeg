								Background="Transparent"
								Visibility="{TemplateBinding DropDownIndicatorVisibility}"
								Template="{StaticResource RadSplitButton_DropDownStyle}"
								IsTabStop="false"                                
								MinWidth="11" MinHeight="7"
								HorizontalAlignment="Stretch"
								
								IsChecked="{TemplateBinding IsOpen}"
								
								 />
							<Rectangle
								x:Name="SeparatorPart"
								Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2"
								MinWidth="1" MinHeight="1"
								Visibility="Collapsed" IsHitTestVisible="False"
								HorizontalAlignment="Stretch" VerticalAlignment="Bottom"
								Fill="{StaticResource RadRibbonBarDropDownButtonPressedBorder}"
								 />

							<Image Name="Image" IsHitTestVisible="False" Margin="2" Grid.ColumnSpan="2" HorizontalAlignment="Center" />

							<TextBlock Name="textRow1" Grid.Row="1" Grid.ColumnSpan="2" Margin="2,0,2,0" IsHitTestVisible="False"
									Text="{TemplateBinding TextRow1}" TextAlignment="Center" />

							<StackPanel Name="DropDownPanel" Grid.Row="1" Grid.ColumnSpan="2" Margin="2,12,0,2"
									HorizontalAlignment="Center"  Orientation="Horizontal"
									MinHeight="14" MaxHeight="14" VerticalAlignment="Center" Height="14"  IsHitTestVisible="False">
								<TextBlock Name="textRow2" Text="{TemplateBinding TextRow2}" Margin="0 -1 0 0" IsHitTestVisible="False"/>
								<Path
									Name="DropDownIndicator"
									IsHitTestVisible="False"
									Visibility="{TemplateBinding DropDownIndicatorVisibility}"
									Fill="{StaticResource RibbonButtonDropDownArrow}"
									Stroke="{StaticResource RibbonButtonDropDownArrowBorder}"
									Data="M 0,0 L 3.5,3.5 L 7,0"
									VerticalAlignment="Center"
									Margin="2,0,2,0"
									 />
							</StackPanel>
						</Grid>
						<Popup
							x:Name="DropDownPopup"
							
							>
							<Grid>
								<!--<telerikRibbonBarPrimitives:DropShadow Margin="6,6,4,4" />-->
								<Border x:Name="DropDownPopupBorder"
										BorderBrush="{StaticResource DropDownPopupBorderBrush}"
										Background="{StaticResource DropDownPopupBackgroundBrush}"
										BorderThickness="1" Padding="1" Margin="0"
										MinWidth="3"
										MinHeight="3"
										Width="{TemplateBinding DropDownWidth}"
										Height="{TemplateBinding DropDownHeight}"
										MaxWidth="{TemplateBinding DropDownMaxWidth}"
										MaxHeight="{TemplateBinding DropDownMaxHeight}">
									<ContentPresenter
										x:Name="DropDownPopupContent"
										Content="{TemplateBinding DropDownContent}"
										ContentTemplate="{TemplateBinding DropDownContentTemplate}"
										HorizontalAlignment="Stretch"
										VerticalAlignment="Stretch"
										DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}" />
								</Border>
							</Grid>
						</Popup>

						<Rectangle x:Name="disabledDecorator" Visibility="Collapsed" Opacity="0" Fill="#FFFFFFFF" />
					</Grid>

					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	
	

	<Style TargetType="telerikRibbonBar:RadRibbonTab"
            >
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="Background" Value="{StaticResource RibbonTabBackgroundBrush}" />
		<Setter Property="BorderBrush" Value="{StaticResource RibbonTabOuterBorderBrush}" />
		<Setter Property="Padding" Value="1,1,1,0" />
		<Setter Property="ItemsPanel">
			<Setter.Value>
				<ItemsPanelTemplate>
					<telerikRibbonBarPrimitives:TabPanel />
				</ItemsPanelTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonBar:RadRibbonTab">
					<Grid>
                        
                        <vsm:VisualStateManager.VisualStateGroups>
							<vsm:VisualStateGroup x:Name="ContextualState">
								<vsm:VisualState x:Name="NotContextual" />
								<vsm:VisualState x:Name="Contextual">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames Storyboard.TargetName="OuterBorder" Storyboard.TargetProperty="BorderBrush">
											<DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource RibbonTabContextualOuterBorderBrush}" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="BorderBrush">
											<DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource RibbonTabContextualInnerBorderBrush}" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
						</vsm:VisualStateManager.VisualStateGroups>
                        
                        <Border Name="OuterBorder" CornerRadius="{StaticResource RibbonTabOuterCornerRadius}" BorderThickness="1" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}">
							<Border Name="InnerBorder" CornerRadius="{StaticResource RibbonTabInnerCornerRadius}" BorderThickness="1" BorderBrush="{StaticResource RibbonTabInnerBorderBrush}">
                                <telerikRibbonBarPrimitives:RibbonScrollViewer>
                                    <ItemsPresenter  HorizontalAlignment="Left" Margin="{TemplateBinding Padding}" />
								</telerikRibbonBarPrimitives:RibbonScrollViewer>
							</Border>
						</Border>
					</Grid>
                    <!---->
                </ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>


	
	

	<Style TargetType="telerikRibbonBar:RadRibbonToggleButton"
		
		>
		<Setter Property="Padding" Value="3" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="HorizontalAlignment" Value="Left" />
		<Setter Property="HorizontalContentAlignment" Value="Left" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonBar:RadRibbonToggleButton">
					<Grid>
						
						<vsm:VisualStateManager.VisualStateGroups>
							<vsm:VisualStateGroup x:Name="CommonStates">
								<vsm:VisualState x:Name="Normal" />
								<vsm:VisualState x:Name="MouseOver">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="BorderBrush"
												Storyboard.TargetName="buttonSurface">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadRibbonBarButtonOverBorder}" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="Background"
												Storyboard.TargetName="buttonSurface">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadRibbonBarButtonOverBackground}" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="BorderBrush"
												Storyboard.TargetName="buttonSurfaceInner">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadRibbonBarButtonOverBorderInner}" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
								<vsm:VisualState x:Name="Pressed">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="BorderBrush"
												Storyboard.TargetName="buttonSurface">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadRibbonBarButtonPressedBorder}" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="Background"
												Storyboard.TargetName="buttonSurface">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadRibbonBarButtonPressedBackground}" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="BorderBrush"
												Storyboard.TargetName="buttonSurfaceInner">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadRibbonBarButtonPressedBorderInner}" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
								<vsm:VisualState x:Name="Disabled">
									<Storyboard Duration="0">
                                        <DoubleAnimation Duration="0" To="0.5"
												Storyboard.TargetName="ContentGrid"
												Storyboard.TargetProperty="Opacity" />
                                    </Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="CheckStates">
								<vsm:VisualState x:Name="Unchecked" />
								<vsm:VisualState x:Name="Checked">
									<Storyboard>
										<DoubleAnimation Duration="0" To="1"
												Storyboard.TargetProperty="Opacity"
												Storyboard.TargetName="mouseOverIndicator" />
									</Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="FocusStates">
								<vsm:VisualState x:Name="Focused" />
								<vsm:VisualState x:Name="Unfocused" />
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="TextAlignment">
								<vsm:VisualState x:Name="TextAtBottom" />
								<vsm:VisualState x:Name="TextAtRight">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="(Grid.Row)">
											<DiscreteObjectKeyFrame KeyTime="0" Value="0" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="(Grid.Column)">
											<DiscreteObjectKeyFrame KeyTime="0" Value="1" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="Margin">
											<DiscreteObjectKeyFrame KeyTime="0" Value="4,0,1,0" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="VerticalAlignment">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Center" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="TextAlignment">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Left" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow2"
												Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
								<vsm:VisualState x:Name="TextHide">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow2"
												Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
						</vsm:VisualStateManager.VisualStateGroups>
						
						<Border x:Name="buttonSurface"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}"
								Background="{TemplateBinding Background}"
								CornerRadius="2">
							<Border x:Name="buttonSurfaceInner"
									BorderBrush="Transparent"
									BorderThickness="{TemplateBinding BorderThickness}" />
						</Border>
						<Border x:Name="mouseOverIndicator" CornerRadius="2" Opacity="0"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{StaticResource RadRibbonBarButtonSelectedBorder}"
								Background="{StaticResource RadRibbonBarButtonSelectedBackground}">
							<Border x:Name="mouseOverIndicatorInner"
									BorderBrush="{StaticResource RadRibbonBarButtonSelectedBorderInner}"
									BorderThickness="{TemplateBinding BorderThickness}" />
						</Border>

                        <Grid Margin="{TemplateBinding Padding}" x:Name="ContentGrid">
							<ContentPresenter
								Name="ContentPresenter"
								Content="{TemplateBinding Content}"
								ContentTemplate="{TemplateBinding ContentTemplate}"
								VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
								HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
								 />

							<Grid>
								<Grid.RowDefinitions>
									<RowDefinition Height="Auto" />
									<RowDefinition Height="*" MaxHeight="28" />
								</Grid.RowDefinitions>
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="Auto" />
									<ColumnDefinition Width="*" />
								</Grid.ColumnDefinitions>

								<Image Name="Image" />

								<TextBlock Name="textRow1" Grid.Row="1" Margin="0,2,0,0"
										Text="{TemplateBinding TextRow1}" TextAlignment="Center" />
								<TextBlock Name="textRow2" Grid.Row="1" Margin="0,14,0,0"
										Text="{TemplateBinding TextRow2}" TextAlignment="Center" />
							</Grid>
						</Grid>

						<Rectangle x:Name="DisabledIndicator" Visibility="Collapsed" Fill="#99FFFFFF" />
					</Grid>

					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	
	
	

	<ControlTemplate x:Key="RibbonButtonTemplateVertical" TargetType="ButtonBase">
		<Grid x:Name="Root" Background="Transparent">
			
			<vsm:VisualStateManager.VisualStateGroups>
				<vsm:VisualStateGroup x:Name="CommonStates">
					<vsm:VisualState x:Name="Normal" />
					<vsm:VisualState x:Name="MouseOver">
						<Storyboard>
							<ObjectAnimationUsingKeyFrames Storyboard.TargetName="OuterBorder"
									Storyboard.TargetProperty="Background">
								<DiscreteObjectKeyFrame KeyTime="0"
										Value="{StaticResource ButtonHotBackgroundBrush}" />
							</ObjectAnimationUsingKeyFrames>
							<ObjectAnimationUsingKeyFrames Storyboard.TargetName="OuterBorder"
									Storyboard.TargetProperty="BorderBrush">
								<DiscreteObjectKeyFrame KeyTime="0"
										Value="{StaticResource ButtonHotOuterBorderBrush}" />
							</ObjectAnimationUsingKeyFrames>
							<ObjectAnimationUsingKeyFrames Storyboard.TargetName="InnerBorder"
									Storyboard.TargetProperty="BorderBrush">
								<DiscreteObjectKeyFrame KeyTime="0"
										Value="{StaticResource ButtonHotInnerBorderBrush}" />
							</ObjectAnimationUsingKeyFrames>
						</Storyboard>
					</vsm:VisualState>
					<vsm:VisualState x:Name="Pressed">
						<Storyboard>
							<ObjectAnimationUsingKeyFrames Storyboard.TargetName="OuterBorder"
									Storyboard.TargetProperty="Background">
								<DiscreteObjectKeyFrame KeyTime="0"
										Value="{StaticResource ButtonPushedBackgroundBrush}" />
							</ObjectAnimationUsingKeyFrames>
							<ObjectAnimationUsingKeyFrames Storyboard.TargetName="OuterBorder"
									Storyboard.TargetProperty="BorderBrush">
								<DiscreteObjectKeyFrame KeyTime="0"
										Value="{StaticResource ButtonPushedOuterBorderBrush}" />
							</ObjectAnimationUsingKeyFrames>
							<ObjectAnimationUsingKeyFrames Storyboard.TargetName="InnerBorder"
									Storyboard.TargetProperty="BorderBrush">
								<DiscreteObjectKeyFrame KeyTime="0"
										Value="{StaticResource ButtonPushedInnerBorderBrush}" />
							</ObjectAnimationUsingKeyFrames>
						</Storyboard>
					</vsm:VisualState>

				</vsm:VisualStateGroup>
			</vsm:VisualStateManager.VisualStateGroups>
			
			<Border Name="OuterBorder" Background="{TemplateBinding Background}"
					BorderBrush="{TemplateBinding BorderBrush}"
					BorderThickness="{TemplateBinding BorderThickness}" CornerRadius="1">
				<Border Name="InnerBorder"
						BorderBrush="{StaticResource PageScrollViewerButtonInnerBorderBrush}"
						BorderThickness="1">
					<ContentPresenter x:Name="ContentPresenter" Content="{TemplateBinding Content}"
							ContentTemplate="{TemplateBinding ContentTemplate}"
							VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
							HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
							Margin="1" />
				</Border>
			</Border>
		</Grid>
		
	</ControlTemplate>

