									<Rectangle Grid.Row="0" Height="14" Fill="{StaticResource ApplicationMenuTopBorderBrush}" />
									<Border Grid.Row="1" Background="{StaticResource ApplicationMenuMiddleBorderBrush}">
										<Border Margin="2,0,2,0" BorderThickness="1" BorderBrush="{StaticResource ApplicationMenuContentOuterBorderBrush}">
											<Border BorderThickness="1" BorderBrush="{StaticResource ApplicationMenuContentInnerBorderBrush}">
												<Grid>
													<Grid.ColumnDefinitions>
														<ColumnDefinition></ColumnDefinition>
														<ColumnDefinition Width="Auto"></ColumnDefinition>
													</Grid.ColumnDefinitions>
													<Border Name="LeftPane" BorderBrush="#c5c5c5" Background="{TemplateBinding Background}" BorderThickness="0,0,1,0">
														<ItemsPresenter />
													</Border>
													<Border Name="RightPane" Visibility="{TemplateBinding RightPaneVisibility}" Grid.Column="1" BorderBrush="{StaticResource ApplicationMenuDividerLightBrush}" Background="{StaticResource ApplicationMenuContentBackgroundBrush}" BorderThickness="1,0,0,0">
														<ContentPresenter Content="{TemplateBinding Content}" Name="ContentPresenter" MinWidth="100" />
													</Border>
												</Grid>
											</Border>
										</Border>
									</Border>
									<Border Grid.Row="2" Background="{StaticResource ApplicationMenuFooterBorderBrush}">
