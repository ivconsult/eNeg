    <!-- ApplicationButtonStyle -->
    <Style x:Key="ApplicationButtonStyle" TargetType="telerikRibbonBar:RadRibbonDropDownButton">
        <Setter Property="Padding" Value="0" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Background" Value="Transparent" />
        
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:RadRibbonDropDownButton">
                    <Grid Width="37" Height="37">
                        
                        <vsm:VisualStateManager.VisualStateGroups>
                            <vsm:VisualStateGroup x:Name="CommonStates">
                                <vsm:VisualState x:Name="Normal">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="MouseOverPart"
												Storyboard.TargetProperty="Opacity" To="0"
												Duration="00:00:00.3" />
                                        <DoubleAnimation Storyboard.TargetName="PressedPart"
												Storyboard.TargetProperty="Opacity" To="0"
												Duration="00:00:00.3" />
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="MouseOverPart"
												Storyboard.TargetProperty="Opacity" To="1"
												Duration="00:00:00.1" />
                                        <DoubleAnimation Storyboard.TargetName="PressedPart"
												Storyboard.TargetProperty="Opacity" To="0"
												Duration="00:00:00.3" />
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="PressedPart"
												Storyboard.TargetProperty="Opacity" To="1"
												Duration="00:00:00.1" />
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="Disabled" />
                            </vsm:VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="FocusStates">
                                <vsm:VisualState x:Name="Focused" />
                                <vsm:VisualState x:Name="Unfocused" />
                            </vsm:VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="TextAlignment">
                                <vsm:VisualState x:Name="TextAtBottom" />
                                <vsm:VisualState x:Name="TextAtRight">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="(Grid.Row)">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="0" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="(Grid.Column)">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="1" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="Margin">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="4,0,1,0" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="VerticalAlignment">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Center" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="TextAlignment">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Left" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow2"
												Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="TextHide">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow2"
												Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="IsOpenState">
                                <vsm:VisualState x:Name="Closed" />
                                <vsm:VisualState x:Name="Opened">
                                    <Storyboard>
                                        <Storyboard>
                                            <DoubleAnimation Storyboard.TargetName="isOpenIndicator"
													Storyboard.TargetProperty="Opacity" To="1"
													Duration="00:00:00.1" />
                                        </Storyboard>
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="SplitState">
                                <vsm:VisualState x:Name="DropDownButtonAtTop" />
                                <vsm:VisualState x:Name="DropDownButtonAtLeft" />
                                <vsm:VisualState x:Name="DropDownButtonAtBottom" />
                                <vsm:VisualState x:Name="DropDownButtonAtRight">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="DropDownPanel"
												Storyboard.TargetProperty="(Grid.Column)">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="2" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="DropDownPanel"
												Storyboard.TargetProperty="(Grid.Row)">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="0" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="DropDownPanel"
												Storyboard.TargetProperty="Margin">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="0,0,0,0" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow2"
												Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="DropDownContentStates">
                                <vsm:VisualState x:Name="HasDropDownContent" />
                                <vsm:VisualState x:Name="WithoutDropDownContent" />
                            </vsm:VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="DropShadowVisibilityStates">
                                <vsm:VisualState x:Name="HasShadow" />
                                <vsm:VisualState x:Name="WithoutShadow" />
                            </vsm:VisualStateGroup>
                        </vsm:VisualStateManager.VisualStateGroups>
                        
                        <Ellipse Margin="-1.5,-1.5,-2,-2">
                            <Ellipse.Fill>
                                <RadialGradientBrush>
                                    <GradientStop Offset="0.9" Color="#71000000" />
                                    <GradientStop Offset="1" Color="Transparent" />
                                </RadialGradientBrush>
                            </Ellipse.Fill>
                        </Ellipse>
                        <Ellipse Fill="{TemplateBinding Background}" />
                        <Grid>
                            <Ellipse StrokeThickness="1"
									Fill="{StaticResource ApplicationMenuButtonBackground}"
									Stroke="{StaticResource ApplicationMenuButtonBorder}" />
                            <Ellipse Margin="1"
									Fill="{StaticResource ApplicationMenuButtonInnerBackground}"
									Stroke="{StaticResource ApplicationMenuButtonInnerBorder}" />
                            <Ellipse Margin="1"
									Fill="{StaticResource ApplicationMenuButtonMiddelBackground}" />
                        </Grid>
                        <Grid x:Name="MouseOverPart" Opacity="0">
                            <Ellipse StrokeThickness="1"
									Fill="{StaticResource ApplicationMenuOverButtonBackground}"
									Stroke="{StaticResource ApplicationMenuOverButtonBorder}" />
                            <Ellipse Margin="1"
									Fill="{StaticResource ApplicationMenuOverButtonInnerBackground}"
									Stroke="{StaticResource ApplicationMenuOverButtonInnerBorder}" />
                            <Ellipse Margin="1"
									Fill="{StaticResource ApplicationMenuOverButtonMiddelBackground}" />
                        </Grid>
                        <Grid x:Name="PressedPart" Opacity="0">
                            <Ellipse StrokeThickness="1"
									Fill="{StaticResource ApplicationMenuPressedButtonBackground}"
									Stroke="{StaticResource ApplicationMenuPressedButtonBorder}" />
                            <Ellipse Margin="1"
									Fill="{StaticResource ApplicationMenuPressedButtonInnerBackground}"
									Stroke="{StaticResource ApplicationMenuPressedButtonInnerBorder}" />
                            <Ellipse Margin="1"
									Fill="{StaticResource ApplicationMenuPressedButtonMiddelBackground}" />
                        </Grid>
                        <Grid x:Name="isOpenIndicator" Opacity="0">
                            <Ellipse StrokeThickness="1"
									Fill="{StaticResource ApplicationMenuPressedButtonBackground}"
									Stroke="{StaticResource ApplicationMenuPressedButtonBorder}" />
                            <Ellipse Margin="1"
									Fill="{StaticResource ApplicationMenuPressedButtonInnerBackground}"
									Stroke="{StaticResource ApplicationMenuPressedButtonInnerBorder}" />
                            <Ellipse Margin="1"
									Fill="{StaticResource ApplicationMenuPressedButtonMiddelBackground}" />
                        </Grid>
                        <Grid Margin="{TemplateBinding Padding}"
								HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
								VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="*" MaxHeight="28" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>

                            <ContentPresenter Grid.ColumnSpan="3" Grid.RowSpan="2"
									HorizontalAlignment="Center" VerticalAlignment="Center"
									Content="{TemplateBinding Content}"
									ContentTemplate="{TemplateBinding ContentTemplate}" />

                            <Image x:Name="Image" Stretch="None" Grid.ColumnSpan="3"
									Grid.RowSpan="2" />

                            <TextBlock x:Name="textRow1" Grid.Row="1" Margin="0,2,0,0"
									Text="{TemplateBinding TextRow1}" TextAlignment="Center" />

                            <StackPanel x:Name="DropDownPanel" Grid.Row="1" Margin="0,14,0,0"
									HorizontalAlignment="Center" Orientation="Horizontal"
									MinHeight="14" MaxHeight="14" Height="14">
                                <TextBlock x:Name="textRow2" Text="{TemplateBinding TextRow2}" />
                            </StackPanel>
                        </Grid>

                        <Popup x:Name="DropDownPopup"
							
                            >
                            <!--PopupAnimation="{DynamicResource {x:Static SystemParameters.ComboBoxPopupAnimationKey}}">-->
                            <Border x:Name="DropDownPopupBorder" BorderBrush="Transparent"
									Background="Transparent" BorderThickness="0" CornerRadius="1"
									Padding="0" Margin="-5,8,0,0"
                                
                                
                                MinWidth="33" MinHeight="33">
                                <ContentControl x:Name="DropDownPopupContent"
										Content="{TemplateBinding DropDownContent}"
										HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                            </Border>
                        </Popup>

                        <Rectangle x:Name="DisabledIndicator" Visibility="Collapsed"
								Fill="#99FFFFFF" />
                    </Grid>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>


    <!--  MinimizeButton -->
    <Style x:Key="MinimizeButtonStyle" TargetType="telerik:RadToggleButton">
        <Setter Property="Margin" Value="1" />
        <Setter Property="Padding" Value="2" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadToggleButton">
                    <Grid Width="20">
                        
                        <vsm:VisualStateManager.VisualStateGroups>
                            <vsm:VisualStateGroup x:Name="CommonStates">
                                <vsm:VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="Content"
												Storyboard.TargetProperty="Opacity" To="0.5"
												Duration="0" />
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="CheckStates">
                                <vsm:VisualState x:Name="Unchecked" />
                                <vsm:VisualState x:Name="Checked" >
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="directionRotation"
									Storyboard.TargetProperty="Angle" To="180" Duration="0"/>
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                        </vsm:VisualStateManager.VisualStateGroups>
                        

                        <telerikChromes:ButtonChrome
								telerik:StyleManager.Theme="{StaticResource Theme}"
								Background="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Background}"
								BorderBrush="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderBrush}"
								BorderThickness="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness}"
								RenderEnabled="{TemplateBinding IsEnabled}" RenderNormal="False"
								RenderMouseOver="{TemplateBinding IsMouseOver}"
								RenderPressed="{TemplateBinding IsPressed}"
								RenderFocused="{TemplateBinding IsFocused}"
								RenderChecked="{TemplateBinding IsChecked}"
								CornerRadius="1" />
                        <Border BorderThickness="{TemplateBinding BorderThickness}">
                            <Grid VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
									HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}">
                                <Path x:Name="arrow" Fill="White" Stretch="None" Stroke="Black" 
                                      Data="M0.25,4 L3.75,0.25 L4.75,0.25 L8.25,3.875 L8.3125,5.5 L7.25,6.627 L6.25,6.627 L4.25,4.627 L2.25,6.627 L1.25,6.627 L0.25,5.627 z" 
                                      StrokeThickness="0.5" RenderTransformOrigin="0.5 0.5">
                                    <Path.RenderTransform>
                                        <RotateTransform x:Name="directionRotation" Angle="0" />
                                    </Path.RenderTransform>
                                </Path>
                                <ContentPresenter x:Name="Content" Margin="{TemplateBinding Padding}"
									Content="{TemplateBinding Content}"
									ContentTemplate="{TemplateBinding ContentTemplate}"/>
                            </Grid>
                        </Border>
                    </Grid>

                    

                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- TabItem  Style -->
    <Style TargetType="telerikNavigation:RadTabItem" x:Key="RibbonRadTabItemStyle">
        <Setter Property="Padding" Value="0,1,0,1" />
        <Setter Property="Margin" Value="0" />
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="1,0,0,0" />
        <Setter Property="BorderBrush" Value="{StaticResource TabStripTabDividerBrush}" />
        
        <Setter Property="TopTemplate">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadTabItem">
                    <Grid>
                        
                        <vsm:VisualStateManager.VisualStateGroups>
                            <vsm:VisualStateGroup x:Name="InteractionState">
                                <vsm:VisualState x:Name="Normal">
                                    <Storyboard>
                                        <ColorAnimation Storyboard.TargetName="LeftOuterBorderBrush"
												Storyboard.TargetProperty="Color" To="Transparent"
												Duration="00:00:00.2" />
                                        <ColorAnimation
												Storyboard.TargetName="RightOuterBorderBrush"
												Storyboard.TargetProperty="Color" To="Transparent"
												Duration="00:00:00.2" />
                                        <ColorAnimation
												Storyboard.TargetName="MiddleOuterBorderBrush"
												Storyboard.TargetProperty="Color" To="Transparent"
												Duration="00:00:00.2" />
                                        <ColorAnimation Storyboard.TargetName="LeftInnerBorderBrush"
												Storyboard.TargetProperty="Color" To="Transparent"
												Duration="00:00:00.2" />
                                        <ColorAnimation
												Storyboard.TargetName="RightInnerBorderBrush"
												Storyboard.TargetProperty="Color" To="Transparent"
												Duration="00:00:00.2" />
                                        <ColorAnimation
												Storyboard.TargetName="MiddleInnerBorderBrush"
												Storyboard.TargetProperty="Color" To="Transparent"
												Duration="00:00:00.2" />
                                        <ColorAnimation Storyboard.TargetName="BackgroundBrush1"
												Storyboard.TargetProperty="Color" To="Transparent"
												Duration="00:00:00.2" />
                                        <ColorAnimation Storyboard.TargetName="BackgroundBrush2"
												Storyboard.TargetProperty="Color" To="Transparent"
												Duration="00:00:00.2" />
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ColorAnimation Storyboard.TargetName="LeftOuterBorderBrush"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabHotOuterBorderColor}"
												Duration="0" />
                                        <ColorAnimation
												Storyboard.TargetName="RightOuterBorderBrush"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabHotOuterBorderColor}"
												Duration="0" />
                                        <ColorAnimation
												Storyboard.TargetName="MiddleOuterBorderBrush"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabHotOuterBorderColor}"
												Duration="0" />
                                        <ColorAnimation Storyboard.TargetName="LeftInnerBorderBrush"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabHotInnerBorderColor}"
												Duration="0" />
                                        <ColorAnimation
												Storyboard.TargetName="RightInnerBorderBrush"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabHotInnerBorderColor}"
												Duration="0" />
                                        <ColorAnimation
												Storyboard.TargetName="MiddleInnerBorderBrush"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabHotInnerBorderColor}"
												Duration="0" />
                                        <ColorAnimation Storyboard.TargetName="BackgroundBrush1"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabHotBackgroundColor1}"
												Duration="0" />
                                        <ColorAnimation Storyboard.TargetName="BackgroundBrush2"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabHotBackgroundColor2}"
												Duration="0" />
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="Selected">
                                    <Storyboard>
                                        <ColorAnimation Storyboard.TargetName="LeftOuterBorderBrush"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabSelectedOuterBorderColor}"
												Duration="0" />
                                        <ColorAnimation
												Storyboard.TargetName="RightOuterBorderBrush"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabSelectedOuterBorderColor}"
												Duration="0" />
                                        <ColorAnimation
												Storyboard.TargetName="MiddleOuterBorderBrush"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabSelectedOuterBorderColor}"
												Duration="0" />
                                        <ColorAnimation Storyboard.TargetName="LeftInnerBorderBrush"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabSelectedLeftInnerBorderColor}"
												Duration="0" />
                                        <ColorAnimation
												Storyboard.TargetName="RightInnerBorderBrush"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabSelectedRightInnerBorderColor}"
												Duration="0" />
                                        <ColorAnimation
												Storyboard.TargetName="MiddleInnerBorderBrush"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabSelectedMiddleInnerBorderColor}"
												Duration="0" />
                                        <ColorAnimation Storyboard.TargetName="BackgroundBrush1"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabSelectedBackgroundColor1}"
												Duration="0" />
                                        <ColorAnimation Storyboard.TargetName="BackgroundBrush2"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabSelectedBackgroundColor2}"
												Duration="0" />
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="wrapper"
												Storyboard.TargetProperty="Margin">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="4,1,3,0" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="LeftShadow"
												Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="RightShadow1"
												Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="RightShadow2"
												Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="HeaderElement"
												Storyboard.TargetProperty="Margin">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="0,1,0,1" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="SelectedMouseOver">
                                    <Storyboard>
                                        <ColorAnimation Storyboard.TargetName="LeftOuterBorderBrush"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabSelectedMouseOverOuterBorderColor}"
												Duration="0" />
                                        <ColorAnimation
												Storyboard.TargetName="RightOuterBorderBrush"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabSelectedMouseOverOuterBorderColor}"
												Duration="0" />
                                        <ColorAnimation
												Storyboard.TargetName="MiddleOuterBorderBrush"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabSelectedMouseOverOuterBorderColor}"
												Duration="0" />
                                        <ColorAnimation Storyboard.TargetName="LeftInnerBorderBrush"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabSelectedMouseOverLeftInnerBorderColor}"
												Duration="0" />
                                        <ColorAnimation
												Storyboard.TargetName="RightInnerBorderBrush"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabSelectedMouseOverRightInnerBorderColor}"
												Duration="0" />
                                        <ColorAnimation
												Storyboard.TargetName="MiddleInnerBorderBrush"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabSelectedMouseOverMiddleInnerBorderColor}"
												Duration="0" />
                                        <ColorAnimation Storyboard.TargetName="BackgroundBrush1"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabSelectedBackgroundColor1}"
												Duration="0" />
                                        <ColorAnimation Storyboard.TargetName="BackgroundBrush2"
												Storyboard.TargetProperty="Color"
												To="{StaticResource TabStripTabSelectedBackgroundColor2}"
												Duration="0" />
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="wrapper"
												Storyboard.TargetProperty="Margin">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="4,1,3,0" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="HeaderElement"
												Storyboard.TargetProperty="Margin">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="0,1,0,1" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetProperty="Opacity"
									Storyboard.TargetName="HeaderElement" To="0.5"
									Duration="0:0:0.1" />
                                    </Storyboard>
                                </VisualState>
                            </vsm:VisualStateGroup>
                        </vsm:VisualStateManager.VisualStateGroups>
                        
                        <Rectangle Margin="0 1 0 4" HorizontalAlignment="Left"
								Fill="{TemplateBinding BorderBrush}" VerticalAlignment="Stretch"
								Width="1" />
                        <Grid x:Name="wrapper" Margin="4,1,3,1" 
                              
                            Background="{TemplateBinding Background}"
                             >

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="3" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="3" />
                            </Grid.ColumnDefinitions>
                            <!--<Border BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" />-->

                            <Rectangle Grid.ColumnSpan="3" Margin="1,1,1,0" x:Name="rect">
                                <Rectangle.Fill>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop x:Name="BackgroundBrush1" Offset="0" />
                                        <GradientStop x:Name="BackgroundBrush2" Offset="1" />
                                    </LinearGradientBrush>
                                </Rectangle.Fill>
                            </Rectangle>
                            <Rectangle Grid.Column="0" Width="1" HorizontalAlignment="Left"
									Margin="-1,3,0,1" x:Name="LeftShadow" Visibility="Collapsed">
                                <Rectangle.Fill>
                                    <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                                        <GradientStop Offset="0" Color="Transparent" />
                                        <GradientStop Offset="0.6" Color="#08000000" />
                                    </LinearGradientBrush>
                                </Rectangle.Fill>
                            </Rectangle>
                            <Path Grid.Column="0" x:Name="LeftOuterBorder"
									Data="M -0.5,24 L 2.5,21 L 2.5,3 L 5.5,0" Margin="-2,0,0,0">
                                <Path.Stroke>
                                    <SolidColorBrush x:Name="LeftOuterBorderBrush" />
                                </Path.Stroke>
                            </Path>
                            <Path Grid.Column="0" x:Name="LeftInnerBorder"
									Data="M -0.5,24 L 2.5,21 L 2.5,3 L 4.5,1" Margin="-1,0,0,0">
                                <Path.Stroke>
                                    <SolidColorBrush x:Name="LeftInnerBorderBrush" />
                                </Path.Stroke>
                            </Path>

                            <Border Grid.Column="1" x:Name="Middle" BorderThickness="0,1,0,0">
                                <Border.BorderBrush>
                                    <SolidColorBrush x:Name="MiddleOuterBorderBrush" />
                                </Border.BorderBrush>
                            </Border>
                            <Rectangle Grid.Column="1" x:Name="MiddleInnerBorder" Height="1"
									VerticalAlignment="Top" Margin="0,1,0,0">
                                <Rectangle.Fill>
                                    <SolidColorBrush x:Name="MiddleInnerBorderBrush" />
                                </Rectangle.Fill>
                            </Rectangle>
                            <telerikNavigationPrimitives:TabItemContentPresenter Grid.Column="1"
									x:Name="HeaderElement" Content="{TemplateBinding Header}"
									ContentTemplate="{TemplateBinding HeaderTemplate}"
									VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
									HorizontalAlignment="Center" HorizontalContentAlignment="Center"
									VerticalAlignment="Center" Margin="0,2,0,1" Foreground="#000000" />
                            <Path Grid.Column="2" x:Name="RightOuterBorder"
									Data="M 5.5,24 L 2.5,21 L 2.5,3 L -0.5,0" Margin="0,0,-2,0">
                                <Path.Stroke>
                                    <SolidColorBrush x:Name="RightOuterBorderBrush" />
                                </Path.Stroke>
                            </Path>
                            <Path Grid.Column="2" x:Name="RightInnerBorder"
									Data="M 4.5,24 L 1.5,21 L 1.5,3 L -0.5,1" Margin="0,0,-1,0">
                                <Path.Stroke>
                                    <SolidColorBrush x:Name="RightInnerBorderBrush" />
                                </Path.Stroke>
                            </Path>
                            <Rectangle Grid.Column="2" Width="1" HorizontalAlignment="Right"
									Margin="0,3,-1,1" x:Name="RightShadow1" Visibility="Collapsed">
                                <Rectangle.Fill>
                                    <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                                        <GradientStop Offset="0" Color="#11000000" />
                                        <GradientStop Offset="1" Color="#15000000" />
                                    </LinearGradientBrush>
                                </Rectangle.Fill>
                            </Rectangle>
                            <Rectangle Grid.Column="2" Width="1" HorizontalAlignment="Right"
									Margin="0,3,-2,1" x:Name="RightShadow2" Visibility="Collapsed">
                                <Rectangle.Fill>
                                    <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                                        <GradientStop Offset="0" Color="Transparent" />
                                        <GradientStop Offset="0.6" Color="#09000000" />
                                    </LinearGradientBrush>
                                </Rectangle.Fill>
                            </Rectangle>
                        </Grid>
                    </Grid>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- WindowTitle -->
    <Style x:Key="WindowTitleStyle" TargetType="telerikRibbonBarPrimitives:WindowTitle">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="FontSize" Value="11" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBarPrimitives:WindowTitle">
                    <StackPanel Orientation="Horizontal">
                        
                        <vsm:VisualStateManager.VisualStateGroups>
                            <vsm:VisualStateGroup x:Name="TitleState">
                                <vsm:VisualState x:Name="Both" />
                                <vsm:VisualState x:Name="TitleOnly">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="ApplicationName"
												Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="Divider"
												Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="ApplicationNameOnly">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Title"
												Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="Divider"
												Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="None">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="ApplicationName"
												Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Title"
												Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="Divider"
												Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                        </vsm:VisualStateManager.VisualStateGroups>
                        
                        <TextBlock x:Name="Title" Text="{TemplateBinding Title}"
								Foreground="{StaticResource RibbonTitlebarTitleBrush}" />
                        <TextBlock x:Name="Divider" Text=" - "
								telerik:LocalizationManager.ResourceKey="RibbonBarWindowTitleDivider"
								Foreground="{StaticResource RibbonTitlebarApplicationNameBrush}" />
                        <TextBlock x:Name="ApplicationName" Text="{TemplateBinding ApplicationName}"
								Foreground="{StaticResource RibbonTitlebarApplicationNameBrush}" />
                    </StackPanel>

                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- Empty ScrollViewer -->
    <Style x:Key="EmptyScrollViewerStyle" TargetType="ScrollViewer">
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="VerticalContentAlignment" Value="Top" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ScrollViewer">
                    <Border CornerRadius="2" BorderBrush="{TemplateBinding BorderBrush}"
							BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid Background="{TemplateBinding Background}">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>

                            <ScrollContentPresenter x:Name="ScrollContentPresenter"
									Cursor="{TemplateBinding Cursor}"
									Margin="{TemplateBinding Padding}"
									ContentTemplate="{TemplateBinding ContentTemplate}" />
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- Scroll Button:-->
    <Style x:Key="TabScrollButtonStyle" TargetType="RepeatButton">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="RepeatButton">
                    <Grid>
                        
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal" />
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="OuterBorder"
												Storyboard.TargetProperty="Background">
                                            <DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource ButtonHotBackgroundBrush}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="OuterBorder"
												Storyboard.TargetProperty="BorderBrush">
                                            <DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource ButtonHotOuterBorderBrush}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="InnerBorder"
												Storyboard.TargetProperty="BorderBrush">
                                            <DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource ButtonHotInnerBorderBrush}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="OuterBorder"
												Storyboard.TargetProperty="Background">
                                            <DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource ButtonPushedBackgroundBrush}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="OuterBorder"
												Storyboard.TargetProperty="BorderBrush">
                                            <DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource ButtonPushedOuterBorderBrush}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="InnerBorder"
												Storyboard.TargetProperty="BorderBrush">
                                            <DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource ButtonPushedInnerBorderBrush}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="OuterBorder"
												Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="FocusStates">
                                <VisualState x:Name="Focused" />
                                <VisualState x:Name="Unfocused" />
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        
                        <Border x:Name="OuterBorder"
								Background="{Binding Background, RelativeSource={RelativeSource TemplatedParent}}"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{Binding BorderBrush, RelativeSource={RelativeSource TemplatedParent}}"
								CornerRadius="3 0 0 3">
                            <Border x:Name="InnerBorder" CornerRadius="2 0 0 2"
									BorderBrush="{StaticResource PageScrollViewerButtonInnerBorderBrush}"
									BorderThickness="1" Padding="{TemplateBinding Padding}">
                                <ContentPresenter Content="{TemplateBinding Content}"
										ContentTemplate="{TemplateBinding ContentTemplate}"
										HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
										VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
                            </Border>
                        </Border>

                    </Grid>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Content" Value="" />
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Path Stretch="Uniform" Height="5" Width="3"
							Data="F1 M 110.692,342.252L 110.692,352.682L 104.594,347.467L 110.692,342.252 Z"
							Fill="{StaticResource PageScrollViewerButtonForegroundBrush}" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Padding" Value="2 2 3 2" />
        <Setter Property="Margin" Value="2" />
        
        <Setter Property="telerik:RadDockPanel.Dock" Value="Left" />
        
        
        <Setter Property="BorderBrush"
				Value="{StaticResource PageScrollViewerButtonOuterBorderBrush}" />
        <Setter Property="Background"
				Value="{StaticResource TabPageScrollViewerButtonBackgroundBrush}" />
        <Setter Property="IsTabStop" Value="False" />
    </Style>



    <!-- TabControl Top Template:-->
    <ControlTemplate x:Key="TabControlTopTemplate" TargetType="telerikNavigation:RadTabControl">
        <Grid>
            
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Disabled" />
                    <VisualState x:Name="Normal" />
                </VisualStateGroup>
                <VisualStateGroup x:Name="DropDownDisplayStates">
                    <VisualState x:Name="DropDownButtonCollapsed" />
                    <VisualState x:Name="DropDownButtonVisible">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames
									Storyboard.TargetName="DropDownButtonElement"
									Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            
            <Grid.RowDefinitions>
                <RowDefinition Height="auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>

            <!--Content-->
            <Border BorderBrush="{TemplateBinding BorderBrush}"
					BorderThickness="{TemplateBinding BorderThickness}"
					 Grid.Row="1">
                
                <ContentPresenter x:Name="ContentElement"
						Content="{TemplateBinding SelectedContent}"
						ContentTemplate="{TemplateBinding SelectedContentTemplate}" />
                
                
            </Border>

            <!--Header Background-->
            <Rectangle Visibility="{TemplateBinding BackgroundVisibility}" />

            <!--TabStrip-->
            
            <telerik:RadDockPanel>
                
                
                    <!-- Items: -->

                    <RepeatButton x:Name="LeftScrollButtonElement"
							Style="{StaticResource TabScrollButtonStyle}" Margin="4 4 2 7"
                              
                        telerik:RadDockPanel.Dock="Left"
                        
                        
                        Visibility="{Binding ComputedHorizontalScrollBarVisibility, ElementName=ScrollViewerElement}" />
                        <RepeatButton x:Name="RightScrollButtonElement"
								RenderTransformOrigin="0.5,0.5"
								Style="{StaticResource TabScrollButtonStyle}" Margin="2 4 2 7"
                               
                            telerik:RadDockPanel.Dock="Right"
                            
                            
                            Visibility="{Binding ComputedHorizontalScrollBarVisibility, ElementName=ScrollViewerElement}" >
                            <RepeatButton.RenderTransform>
                                <ScaleTransform ScaleX="-1" />
                            </RepeatButton.RenderTransform>
                        </RepeatButton>

                        <ScrollViewer x:Name="ScrollViewerElement"
								Style="{StaticResource EmptyScrollViewerStyle}" Margin="2 4 2 0"
								VerticalScrollBarVisibility="Disabled"
								HorizontalScrollBarVisibility="{Binding (ScrollViewer.HorizontalScrollBarVisibility), RelativeSource={RelativeSource TemplatedParent}}">
                            <ItemsPresenter />
                        </ScrollViewer>
                        
            </telerik:RadDockPanel>
            
            

        </Grid>
        
    </ControlTemplate>

    <!-- RadTabControl -->
    <Style TargetType="telerikNavigation:RadTabControl" x:Key="customType">
        <Setter Property="MinWidth" Value="5" />
        <Setter Property="MinHeight" Value="0" />
        <Setter Property="MaxHeight" Value="26" />
        <Setter Property="Foreground" Value="#FF000000" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="IsEnabled" Value="True" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="BorderThickness" Value="1 0 1 1" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <telerikNavigationPrimitives:TabStripPanel x:Name="TabStripPanel"
							Margin="-2 0 -2 0" />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto" />
        <Setter Property="Template" Value="{StaticResource TabControlTopTemplate}" />
        <Setter Property="TopTemplate" Value="{StaticResource TabControlTopTemplate}" />
    </Style>

    <Style TargetType="telerikRibbonBar:QuickAccessToolBar"
            >
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Background" Value="{StaticResource QatBelowBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{StaticResource QatBelowOuterBorderBrush}" />
        <Setter Property="Margin" Value="0" />
        <Setter Property="CustomizationMenuVisibility" Value="Visible" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:QuickAccessToolBar">
                    <Grid MinHeight="22" x:Name="RootElement">
                        
                        <vsm:VisualStateManager.VisualStateGroups>
                            <vsm:VisualStateGroup x:Name="CommonStateGroup">
                                <vsm:VisualState x:Name="Normal" />
                                <vsm:VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetProperty="Opacity"
												Storyboard.TargetName="RootElement" To="0.5"
												Duration="0:0:0.2" />
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="HostingState">
                                <vsm:VisualState x:Name="NotHosted" />
                                <vsm:VisualState x:Name="HostedAboveRibbon" />
                                <vsm:VisualState x:Name="HostedBelowRibbon">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="AboveBackgroundGrid"
												Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="BelowBackgroundGrid"
												Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="PART_StripPanelGrid"
												Storyboard.TargetProperty="Margin">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="2,2,0,2" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <!--<ObjectAnimationUsingKeyFrames Storyboard.TargetName="ToolBarPanel" Storyboard.TargetProperty="Margin">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="2" />
                                        </ObjectAnimationUsingKeyFrames>-->
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="TogglePositionMenuItem"
												Storyboard.TargetProperty="Header">
                                            <DiscreteObjectKeyFrame KeyTime="0"
													Value="Show Above the Ribbon."
													telerik:LocalizationManager.ResourceKey="RibbonBarQATShowAbove" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                        </vsm:VisualStateManager.VisualStateGroups>
                        
                        <Grid x:Name="BelowBackgroundGrid" Visibility="Collapsed">
                            <Border BorderThickness="1" CornerRadius="2 2 0 0"
									BorderBrush="{StaticResource QatBelowInnerBorderBrush}"
									Margin="1,1,0,0" Background="{TemplateBinding Background}" />
                            <Border BorderThickness="1" CornerRadius="2 2 0 0"
									BorderBrush="{TemplateBinding BorderBrush}" Margin="0,0,1,1" />
                        </Grid>

                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="13" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid x:Name="AboveBackgroundGridWrapper">
                                <Grid x:Name="AboveBackgroundGrid" Height="22">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="14" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="10" />
                                        <ColumnDefinition Width="5" />
                                    </Grid.ColumnDefinitions>
                                    <Border Grid.Column="1"
											BorderBrush="{StaticResource QatAboveHighlightBrush}"
											BorderThickness="0,1,0,1" />
                                    <Border Grid.Column="1"
											Background="{StaticResource QatAboveBackgroundBrush}"
											BorderBrush="{StaticResource QatAboveBorderBrush}"
											BorderThickness="0,1,0,1" Margin="0,1,0,1" />
                                    <Path Grid.Column="0"
											Data="M 14,0.5 L -0.5,0.5 A 28,28 30 0 1 12,23.5 L 14,23.5"
											Stroke="{StaticResource QatAboveHighlightBrush}"
											StrokeThickness="1" />
                                    <Path Grid.Column="0"
											Data="M 14,1.5 L 1,1.5 A 27,27 30 0 1 13,22.5 L 14,22.5"
											Fill="{StaticResource QatAboveBackgroundBrush}"
											Stroke="{StaticResource QatAboveBorderBrush}"
											StrokeThickness="1" />
                                    <Path Grid.Column="2" Data="M0,0.5 A9,10,180,1,1,0,21.5"
											Stroke="{StaticResource QatAboveHighlightBrush}"
											StrokeThickness="1" />
                                    <Path Grid.Column="2" Data="M 0,1.5 A 8,10 180 1 1 0,22.5"
											Fill="{StaticResource QatAboveBackgroundBrush}"
											Stroke="{StaticResource QatAboveBorderBrush}"
											StrokeThickness="1" />
                                </Grid>
                            </Grid>
                            <Grid HorizontalAlignment="Left" x:Name="PART_StripPanelGrid"
									Grid.Column="0" Margin="16,0,22,0">
                                <StackPanel x:Name="PART_StripPanel" Orientation="Horizontal"
										HorizontalAlignment="Left" />
                            </Grid>

                            <Popup Grid.Column="1" x:Name="PART_Overflow">

                                <Border Margin="0" MinWidth="55" MinHeight="33">
                                    <Border BorderBrush="{StaticResource OverflowBorderInnerBrush}"
											Background="{StaticResource OverflowContentBackgroundBrush}"
											BorderThickness="1" Padding="1,2,1,2">

                                        <StackPanel x:Name="PART_OverflowPanel"
												Orientation="Vertical" VerticalAlignment="Top" />
                                    </Border>
                                </Border>
                            </Popup>

                            <telerikRibbonBar:RadRibbonDropDownButton Visibility="{TemplateBinding CustomizationMenuVisibility}"  telerik:StyleManager.Theme="{StaticResource Theme}"
									DropDownIndicatorVisibility="Collapsed" Width="13" Height="22"
									HorizontalAlignment="Left" Grid.Column="1"
									VerticalAlignment="Top" Padding="0,2,0,2">
                                <telerikRibbonBar:RadRibbonDropDownButton.DropDownContent>
                                    <StackPanel>
                                        <telerikRibbonBar:RadGroupHeader
												Content="Customize Quick Access Toolbar"
												telerik:LocalizationManager.ResourceKey="RibbonBarQATCustomize" />

                                        <telerikNavigation:RadContextMenu telerik:StyleManager.Theme="{StaticResource Theme}" BorderThickness="0">
                                            <telerikNavigation:RadMenuItem
													x:Name="TogglePositionMenuItem"
													Header="Show Below the Ribbon."
													telerik:LocalizationManager.ResourceKey="RibbonBarQATShowBelow" />
                                            <telerikNavigation:RadMenuItem IsSeparator="True"
													Padding="0" Margin="0" />
                                            <telerikNavigation:RadMenuItem
													x:Name="ToggleMinimizeMenuItem"
													Header="Minimize the Ribbon."
													telerik:LocalizationManager.ResourceKey="RibbonBarQATMinimize" />
                                        </telerikNavigation:RadContextMenu>

                                    </StackPanel>
                                </telerikRibbonBar:RadRibbonDropDownButton.DropDownContent>
                                <Path x:Name="MainPopupButtonPath" HorizontalAlignment="Center"
										VerticalAlignment="Center" Margin="4,7,2,0"
										Data="M 0,-2.5 L 0,-1.5 L 7,-1.5 L 7,-2.5 M 0,0 L 3.5,3.5 L 7,0"
										Fill="{StaticResource QatButtonArrow}"
										Stroke="{StaticResource QatButtonArrowHighlight}" />
                            </telerikRibbonBar:RadRibbonDropDownButton>

                        </Grid>
                    </Grid>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="telerikRibbonBar:RadRibbonContextualGroup" x:Key="ContextualTabsHeaderstyle">
        <Setter Property="Foreground" Value="{StaticResource ContextualTabsHeader_Foreground}" />
        <Setter Property="Template" >
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:RadRibbonContextualGroup">
                    <Grid Margin="0 -1"
                        
                         >
                        <Border BorderThickness="1 0" BorderBrush="#737b87" Margin="-0.5 0 0 2"/>
                        <Border Margin="1" Background="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Background}" >
                            <Border.OpacityMask>
                                <RadialGradientBrush Center="0.507,1.136" GradientOrigin="0.5,1.136" RadiusX="0.736" RadiusY="1.08">
                                    <GradientStop Offset="1"/>
                                    <GradientStop Color="#B2FFFFFF" Offset="0.325"/>
                                </RadialGradientBrush>
                            </Border.OpacityMask>
                        </Border>
                        <Border BorderThickness="0 0 0 1" BorderBrush="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Background}" Margin="0 0 0 1" VerticalAlignment="Bottom">
                            <Border.OpacityMask>
                                <LinearGradientBrush EndPoint="0.007,0.986" StartPoint="1.006,0.986">
                                    <GradientStop Offset="0" Color="#26000000"/>
                                    <GradientStop Color="White" Offset="0.5"/>
                                    <GradientStop Offset="1" Color="#26000000"/>
                                </LinearGradientBrush>
                            </Border.OpacityMask>
                        </Border>
                        <ContentPresenter Margin="10 0 0 0"  Content="{TemplateBinding Header}" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- TabItem  Style -->



    <!-- RibbonBar -->
    <Style TargetType="telerikRibbonBar:RadRibbonBar"
		 >
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Background" Value="{StaticResource RibbonBackgroundBrush}" />
        <Setter Property="Foreground" Value="{StaticResource RibbonForegroundBrush}" />
        <Setter Property="ApplicationButtonVisibility" Value="Visible" />
        <Setter Property="TitleBarVisibility" Value="Visible" />
        <Setter Property="HelpButtonVisibility" Value="Collapsed" />
        <Setter Property="MinimizeButtonVisibility" Value="Collapsed" />
        <Setter Property="ApplicationName" Value="My Application" />
        <Setter Property="IsMinimizable" Value="True" />
        <Setter Property="IsMouseWheelTabScrollingEnabled" Value="True" />
        <Setter Property="QuickAccessToolBarPosition" Value="AboveRibbon" />
        <Setter Property="VerticalAlignment" Value="Top" />
        <Setter Property="CollapseThresholdSize" Value="300, 250" />
        <Setter Property="FontSize" Value="10.8" />
        <Setter Property="ApplicationButtonStyle" Value="{StaticResource ApplicationButtonStyle}" />

        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:RadRibbonBar">
                    <Grid x:Name="RootElement"  >
                        
                        <vsm:VisualStateManager.VisualStateGroups>
                            <vsm:VisualStateGroup x:Name="MinimizedState">
                                <vsm:VisualState x:Name="Minimized" />
                                <vsm:VisualState x:Name="Expanded">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="TabContentHost"
												Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="MinimizedDivider"
												Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                        </vsm:VisualStateManager.VisualStateGroups>
                        
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" MaxHeight="28" />
                            <RowDefinition Height="Auto" MaxHeight="24" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid.Resources>
                            <Style TargetType="telerikNavigation:RadTabItem" x:Key="ContextualTabsStyle">
                                <Setter Property="Padding" Value="0,1,0,1" />
                                <Setter Property="Margin" Value="0" />
                                <Setter Property="Background" Value="Pink"/>
                                <Setter Property="BorderThickness" Value="1,0,0,0" />
                                <Setter Property="Foreground" Value="Pink"/>
                                <Setter Property="BorderBrush" Value="{StaticResource TabStripTabDividerBrush}" />
                                
                                <Setter Property="TopTemplate">
                                    <Setter.Value>
                                        <ControlTemplate TargetType="telerikNavigation:RadTabItem">
                                            <Grid x:Name="wrapper" Margin="1,1,0,1" >
                                                
                                                <vsm:VisualStateManager.VisualStateGroups>
                                                    <VisualStateGroup x:Name="CommonStateGroup">
                                                        <VisualState x:Name="MouseOver" >
                                                            <Storyboard>
                                                                <DoubleAnimation Duration="0" Storyboard.TargetName="MouseOverVisual"
									Storyboard.TargetProperty="(UIElement.Opacity)" To="1" />
                                                            </Storyboard>
                                                        </VisualState>
                                                        <VisualState x:Name="Normal" />
                                                        <VisualState x:Name="Selected" >
                                                            <Storyboard>
                                                                <DoubleAnimation Duration="0" Storyboard.TargetName="SelectionVisual"
									Storyboard.TargetProperty="(UIElement.Opacity)" To="1" />
                                                                <DoubleAnimation Duration="0" Storyboard.TargetName="MouseOverVisual"
									Storyboard.TargetProperty="(UIElement.Opacity)" To="0" />
                                                            </Storyboard>
                                                        </VisualState>
                                                        <VisualState x:Name="SelectedMouseOver" >
                                                            <Storyboard>
                                                                <DoubleAnimation Duration="0" Storyboard.TargetName="MouseOverVisual"
									Storyboard.TargetProperty="(UIElement.Opacity)" To="0" />
                                                                <DoubleAnimation Duration="0" Storyboard.TargetName="SelectionVisual"
									Storyboard.TargetProperty="(UIElement.Opacity)" To="1" />
                                                                <ObjectAnimationUsingKeyFrames Storyboard.TargetName="InnerBackground"
									Storyboard.TargetProperty="Background" Duration="0">
                                                                    <DiscreteObjectKeyFrame KeyTime="0"
										Value="#fbe0a1" />
                                                                </ObjectAnimationUsingKeyFrames>
                                                            </Storyboard>
                                                        </VisualState>
                                                        <VisualState x:Name="Disabled" >
                                                            <Storyboard>
                                                                <DoubleAnimation Storyboard.TargetProperty="Opacity"
									                                Storyboard.TargetName="HeaderElement" To="0.5"
									                                Duration="0:0:0.1" />
                                                            </Storyboard>
                                                        </VisualState>
                                                    </VisualStateGroup>
                                                </vsm:VisualStateManager.VisualStateGroups>
                                                

                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="3" />
                                                    <ColumnDefinition Width="*" />
                                                    <ColumnDefinition Width="3" />
                                                </Grid.ColumnDefinitions>

                                                <Border BorderBrush="#848484" Grid.ColumnSpan="3" Opacity="0"   x:Name="MouseOverVisual" BorderThickness="1 1 1 0" CornerRadius="3 3 0 0" >
                                                    <Border CornerRadius="2 2 0 0" BorderBrush="#99FFFFFF" BorderThickness="1 1 1 0">
                                                        <Border CornerRadius="1 1 0 0" Opacity="0.6"  Background="{TemplateBinding Background}">
                                                            <Border.OpacityMask>
                                                                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                                                    <GradientStop Color="#00FFFFFF" Offset="0"/>
                                                                    <GradientStop Color="#ffFFFFFF" Offset="1"/>
                                                                    <GradientStop Color="#19FFFFFF" Offset="0.527"/>
                                                                </LinearGradientBrush>
                                                            </Border.OpacityMask>
                                                        </Border>
                                                    </Border>
                                                </Border>

                                                <Border BorderBrush="#848484" Grid.ColumnSpan="3" Opacity="0"   x:Name="SelectionVisual"  BorderThickness="1 1 1 0" 
							                        CornerRadius="3 3 0 0" Background="#FFFFFFFF" >
                                                    <Border CornerRadius="2 2 0 0" x:Name="InnerBackground" Background="{TemplateBinding Background}">
                                                        <Border.OpacityMask>
                                                            <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                                                <GradientStop Color="#19FFFFFF" Offset="0"/>
                                                                <GradientStop Color="#59FFFFFF" Offset="0.173"/>
                                                                <GradientStop Color="#7FFFFFFF" Offset="0.191"/>
                                                                <GradientStop Color="Transparent" Offset="0.866"/>
                                                            </LinearGradientBrush>
                                                        </Border.OpacityMask>
                                                    </Border>
                                                </Border>


                                                <telerikNavigationPrimitives:TabItemContentPresenter Grid.Column="1"
									x:Name="HeaderElement" Content="{TemplateBinding Header}"
									ContentTemplate="{TemplateBinding HeaderTemplate}"
									VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
									HorizontalAlignment="Center" HorizontalContentAlignment="Center"
									VerticalAlignment="Center" Margin="0,2,0,1" Foreground="#000000" />


                                            </Grid>
                                            
                                        </ControlTemplate>
                                    </Setter.Value>
                                </Setter>
                            </Style>
                        </Grid.Resources>
                        <!-- Background Fill -->
                        <Rectangle Grid.Row="1" Grid.RowSpan="3" Grid.ColumnSpan="2"
								Fill="{TemplateBinding Background}" />
                        <!-- Titlebar background -->
                        <Border Grid.ColumnSpan="2" x:Name="titleBarBackground"
								BorderThickness="0,0,0,1" 
                                Visibility="{TemplateBinding TitleBarVisibility}"
								BorderBrush="{StaticResource RibbonTitlebarBorderBrush}"
								Background="{StaticResource RibbonTitlebarBackgroundBrush}" />
                        <!-- Tabstrip background -->
                        <Rectangle Grid.Row="1" Grid.ColumnSpan="2" VerticalAlignment="Top"
                                Visibility="{TemplateBinding TitleBarVisibility}"
								Height="1" Fill="{StaticResource RibbonTabstripBorderBrush}" />

                        <Grid x:Name="TitleAligner" Grid.Row="0" Grid.Column="1" Height="28"
								Visibility="{TemplateBinding TitleBarVisibility}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <ContentPresenter x:Name="AboveToolBarHost" Margin="-7,0,0,1"
									VerticalAlignment="Bottom" HorizontalAlignment="Left" />
                            
                            <telerikRibbonBarPrimitives:RibbonTitlePanel x:Name="TitleHost" Grid.Column="2">
                                <telerikRibbonBarPrimitives:RibbonTitlePanel.ContextualGroupPlaceholder>
                                    <telerik:ItemsControl x:Name="ContextPanel" ItemContainerStyle="{StaticResource ContextualTabsHeaderstyle}">
                                        <telerik:ItemsControl.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <telerikRibbonBarPrimitives:ContextualGroupPanel />
                                            </ItemsPanelTemplate>
                                        </telerik:ItemsControl.ItemsPanel>
                                    </telerik:ItemsControl>
                                </telerikRibbonBarPrimitives:RibbonTitlePanel.ContextualGroupPlaceholder>
                                <telerikRibbonBarPrimitives:RibbonTitlePanel.TitlePlaceholder>
                                    <telerikRibbonBarPrimitives:WindowTitle x:Name="PART_ActualWindowTitle" HorizontalAlignment="Center" IsActive="True" Title="{TemplateBinding Title}" ApplicationName="{TemplateBinding ApplicationName}" Style="{StaticResource WindowTitleStyle}" />
                                </telerikRibbonBarPrimitives:RibbonTitlePanel.TitlePlaceholder>
                            </telerikRibbonBarPrimitives:RibbonTitlePanel>
                            
                        </Grid>

                        <!-- Tabstrip and help button -->
                        <Grid Grid.Row="1" Grid.Column="1" Canvas.ZIndex="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>

                            <!--ItemContainerStyle="{StaticResource RibbonRadTabItemStyle}"-->
                            <telerikNavigation:RadTabControl BackgroundVisibility="Collapsed"  ItemContainerStyle="{StaticResource RibbonRadTabItemStyle}"
									Style="{StaticResource customType}"
									x:Name="TabControl" Margin="10,-3,0,0" 			
									ScrollViewer.HorizontalScrollBarVisibility="Auto"
									ScrollMode="Pixel"
                                 >

                                <telerikNavigation:RadTabControl.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <telerikRibbonBarPrimitives:TabStripPanel
												SeparatorOpacityThreshold="3"
												VariablePaddingLength="15" />
                                    </ItemsPanelTemplate>
                                </telerikNavigation:RadTabControl.ItemsPanel>
                            </telerikNavigation:RadTabControl>

                            <StackPanel Grid.Column="1" Orientation="Horizontal">

                                <telerikRibbonBar:RadRibbonToggleButton
                                        Style="{StaticResource MinimizeButtonStyle}"
										Visibility="{TemplateBinding MinimizeButtonVisibility}"
										Size="Small" 
                                        x:Name="MinimizeButton" />

                                <telerikRibbonBar:RadRibbonButton 
                                        telerik:StyleManager.Theme="{StaticResource Theme}"
										Visibility="{TemplateBinding HelpButtonVisibility}"
										x:Name="HelpButton" Size="Small" Height="21"
										SmallImage="{TemplateBinding HelpButtonImageSource}"
										VerticalAlignment="Center" />

                                <ContentPresenter Margin="0"  Content="{TemplateBinding TabStripAdditionalContent}" />
                            </StackPanel>
                        </Grid>
                        
                        <!-- Application button -->
                        <telerikRibbonBar:RadRibbonDropDownButton x:Name="ApplicationButton" Grid.RowSpan="2" Margin="4,4,0,5" DropDownIndicatorVisibility="Collapsed"
                            SmallImage="{TemplateBinding ApplicationButtonImageSource}" Content="{TemplateBinding ApplicationButtonContent}" IsAutoSize="False" Style="{TemplateBinding ApplicationButtonStyle}" Visibility="{TemplateBinding ApplicationButtonVisibility}" />

                        <!-- Tab content -->
                        <Border Canvas.ZIndex="0" x:Name="TabContentHost" Grid.Row="2"
								Grid.ColumnSpan="2"
								Background="{StaticResource RibbonLowerBackgroundBrush}"
								Visibility="Collapsed">
                            <Grid>
                                
                                <ItemsPresenter Visibility="Collapsed"/>
                                

                                <ContentPresenter x:Name="SelectedTabPresenter" Margin="1,-1,1,2"
										Height="91.0" />
                                <Rectangle VerticalAlignment="Bottom" Margin="4,0,4,1" Height="1"
										Fill="#0C000000" />
                            </Grid>
                        </Border>

                        <Popup x:Name="TabContentPopup" VerticalOffset="-2" 
                                >
                        </Popup>

                        <!-- RadToolBar -->
                        <Border x:Name="MinimizedDivider" Height="2" Grid.Row="3"
								Grid.ColumnSpan="2" VerticalAlignment="Top"
								BorderThickness="0,1,0,0" 
								BorderBrush="{StaticResource RibbonDividerDarkBrush}"
								Background="{StaticResource RibbonDividerLightBrush}" />
                        <ContentPresenter x:Name="BelowToolBarHost" Grid.Row="3" Grid.ColumnSpan="2" />
                    </Grid>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    
	

	<Style TargetType="telerikRibbonBar:RadRibbonButton"
		
		>
		<Setter Property="Padding" Value="3" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="HorizontalAlignment" Value="Left" />
		<Setter Property="HorizontalContentAlignment" Value="Left" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonBar:RadRibbonButton">
					<Grid >
						
						<vsm:VisualStateManager.VisualStateGroups>
							<vsm:VisualStateGroup x:Name="CommonStates">
								<vsm:VisualState x:Name="Normal" />
								<vsm:VisualState x:Name="MouseOver">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="BorderBrush"
												Storyboard.TargetName="buttonSurface">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadRibbonBarButtonOverBorder}" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="Background"
												Storyboard.TargetName="buttonSurface">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadRibbonBarButtonOverBackground}" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="BorderBrush"
												Storyboard.TargetName="buttonSurfaceInner">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadRibbonBarButtonOverBorderInner}" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
								<vsm:VisualState x:Name="Pressed">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="BorderBrush"
												Storyboard.TargetName="buttonSurface">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadRibbonBarButtonPressedBorder}" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="Background"
												Storyboard.TargetName="buttonSurface">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadRibbonBarButtonPressedBackground}" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="BorderBrush"
												Storyboard.TargetName="buttonSurfaceInner">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadRibbonBarButtonPressedBorderInner}" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
								<vsm:VisualState x:Name="Disabled">
									<Storyboard Duration="0">
                                        <DoubleAnimation Duration="0" To="0.5"
												Storyboard.TargetName="ContentGrid"
												Storyboard.TargetProperty="Opacity" />
									</Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="FocusStates">
								<vsm:VisualState x:Name="Focused" />
								<vsm:VisualState x:Name="Unfocused" />
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="TextAlignment">
								<vsm:VisualState x:Name="TextAtBottom" />
								<vsm:VisualState x:Name="TextAtRight">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="(Grid.Row)">
											<DiscreteObjectKeyFrame KeyTime="0" Value="0" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="(Grid.Column)">
											<DiscreteObjectKeyFrame KeyTime="0" Value="1" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="Margin">
											<DiscreteObjectKeyFrame KeyTime="0" Value="4,0,1,0" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="VerticalAlignment">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Center" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="TextAlignment">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Left" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow2"
												Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
								<vsm:VisualState x:Name="TextHide">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow2"
												Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
						</vsm:VisualStateManager.VisualStateGroups>
						

						<Border x:Name="buttonSurface"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}"
								Background="{TemplateBinding Background}"
								CornerRadius="2">
							<Border x:Name="buttonSurfaceInner"
									BorderBrush="Transparent"
									BorderThickness="{TemplateBinding BorderThickness}" />
						</Border>

                        <Grid Margin="{TemplateBinding Padding}" x:Name="ContentGrid">
							<ContentPresenter
								Name="ContentPresenter"
								Content="{TemplateBinding Content}"
								ContentTemplate="{TemplateBinding ContentTemplate}"
								VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
								HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
								 />

							<Grid>
								<Grid.RowDefinitions>
									<RowDefinition Height="Auto"/>
									<RowDefinition Height="*" MaxHeight="28"/>
								</Grid.RowDefinitions>
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="Auto" />
									<ColumnDefinition Width="*" />
								</Grid.ColumnDefinitions>

								<Image Name="Image"/>

								<TextBlock Name="textRow1" Grid.Row="1" Margin="0,2,0,0"
									Text="{TemplateBinding TextRow1}" TextAlignment="Center" />
								<TextBlock Name="textRow2" Grid.Row="1" Margin="0,14,0,0"
									Text="{TemplateBinding TextRow2}" TextAlignment="Center" />
							</Grid>
						</Grid>
					</Grid>

					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	
	

	<!-- For  ComboBox, ... -->
	<Style TargetType="telerik:PickerToggleButton" x:Key="ToogleButtonStyle" >
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="BorderBrush" Value="{StaticResource ExpanderButtonNormal_Border}" />
		<Setter Property="Background" Value="{StaticResource ExpanderButtonNormal_Background}" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerik:PickerToggleButton">
					<Grid>
                        
						<vsm:VisualStateManager.VisualStateGroups>
							<vsm:VisualStateGroup x:Name="CommonStates">
								<vsm:VisualState x:Name="Normal" />
								<vsm:VisualState x:Name="MouseOver">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames Storyboard.TargetName="Chrome" 
																		   Storyboard.TargetProperty="BorderBrush">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource ExpandButtonOver_OuterBorder}" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames Storyboard.TargetName="ChromeInner" 
																		   Storyboard.TargetProperty="BorderBrush">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource ExpanderButtonOver_Border}" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames Storyboard.TargetName="ChromeInner" 
																		   Storyboard.TargetProperty="Background">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource ExpanderButtonOver_Background}" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
								<vsm:VisualState x:Name="Pressed">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="Chrome"
												Storyboard.TargetProperty="BorderBrush">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource ExpandButtonPressed_OuterBorder}" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="ChromeInner"
												Storyboard.TargetProperty="BorderBrush">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource ExpanderButtonPressed_Border}" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="ChromeInner"
												Storyboard.TargetProperty="Background">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource ExpanderButtonPressed_Background}" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
								<vsm:VisualState x:Name="Disabled" />
								<vsm:VisualState x:Name="Checked">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="ChromeCheck"
												Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>

								<vsm:VisualState x:Name="Unhecked">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="ChromeCheck"
												Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="FocusStates">
								<vsm:VisualState x:Name="Unfocused" />
								<vsm:VisualState x:Name="Focused" />
							</vsm:VisualStateGroup>
						</vsm:VisualStateManager.VisualStateGroups>
						
						<Border x:Name="Chrome"
								BorderBrush="{TemplateBinding BorderBrush}"
								BorderThickness="1">
							<Border x:Name="ChromeInner"
									BorderBrush="{TemplateBinding Background}"
									BorderThickness="1"
									Background="{TemplateBinding Background}" />
						</Border>

						<Border x:Name="ChromeCheck" Visibility="Collapsed"
								BorderBrush="{StaticResource ExpandButtonPressed_OuterBorder}"
								BorderThickness="1">
							<Border BorderBrush="{StaticResource ExpanderButtonPressed_Border}"
									BorderThickness="1"
									Background="{StaticResource ExpanderButtonPressed_Background}" />
						</Border>

						<Path x:Name="Path"
								Data="M-0.7,0.45 L6.2,0.45 L2.814,5.509 L2.814,5.509 z" 
								Fill="{StaticResource RibbonGroupButtonArrow}" 
								Stretch="Fill" Width="5" Height="3"
								HorizontalAlignment="Right"
								Margin="0 0 4 0"
								VerticalAlignment="Center" />

						<ContentPresenter Content="{TemplateBinding Content}"
								ContentTemplate="{TemplateBinding ContentTemplate}" />

					</Grid>

					

				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<!-- NonEditableComboBoxTemplate -->
	<ControlTemplate x:Key="NonEditableComboBox" TargetType="telerikRibbonBar:RadRibbonComboBox">
		<Grid Name="MainGrid">
            
			<vsm:VisualStateManager.VisualStateGroups>
				<vsm:VisualStateGroup x:Name="CommonStates">
					<vsm:VisualState x:Name="Normal" />
					<vsm:VisualState x:Name="MouseOver" />
					<vsm:VisualState x:Name="Focused" />
					<vsm:VisualState x:Name="Disabled">
						<Storyboard Duration="0">
							<ObjectAnimationUsingKeyFrames Storyboard.TargetName="DisabledVisual"
									Storyboard.TargetProperty="Visibility">
								<DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
							</ObjectAnimationUsingKeyFrames>
						</Storyboard>
					</vsm:VisualState>
				</vsm:VisualStateGroup>
			</vsm:VisualStateManager.VisualStateGroups>
			

			<Border x:Name="ContentPresenterBorder" Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}">
				<Grid>
					<Grid.ColumnDefinitions>
						<ColumnDefinition  Width="*"/>
						<ColumnDefinition Width="20" />
					</Grid.ColumnDefinitions>

					<telerik:PickerToggleButton Margin="-1" x:Name="PART_DropDownButton"
                                Grid.ColumnSpan="2"
								Style="{StaticResource ToogleButtonStyle}"
								IsChecked="{TemplateBinding IsDropDownOpen}"
								IsEnabled="{TemplateBinding IsEnabled}" />


					<ContentPresenter x:Name="ContentPresenterElement"
								HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
								Margin="{TemplateBinding Padding}"
								VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
								IsHitTestVisible="False"
								ContentTemplate="{TemplateBinding SelectionBoxItemTemplate}"
								Content="{TemplateBinding SelectionBoxItem}" />
				</Grid>
			</Border>

			<Popup x:Name="PART_Popup" 
                 >
				<Border x:Name="DropDownBorder"
						HorizontalAlignment="Stretch"
						MinWidth="{TemplateBinding MinDropDownWidth}"
						MaxHeight="{TemplateBinding MaxDropDownHeight}">
					<Grid>
						<Border Background="{StaticResource RibbonPopupBackgroundBrush}" 
								BorderThickness="1"
                                BorderBrush="{StaticResource RibbonPopupOuterBorderBrush}" >
							<Border BorderBrush="{StaticResource RibbonPopupInnerBorderBrush}" BorderThickness="1">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <telerik:RadButton x:Name="PART_ClearButton" Grid.Row="0"
								            Margin="-1 -1 -1 0"
								            Visibility="{TemplateBinding ClearSelectionButtonVisibility}"
								            Content="{TemplateBinding ClearSelectionButtonContent}"
								            telerik:StyleManager.Theme="{StaticResource Theme}" />

                                    <ScrollViewer x:Name="PART_ScrollViewer" Padding="0 1" Grid.Row="1"
											VerticalScrollBarVisibility="Auto" BorderThickness="0"
                                            telerik:StyleManager.Theme="{StaticResource Theme}"
                                            
                                            telerik:ScrollViewerExtensions.EnableMouseWheel="True"
                                            
                                            >
                                        
                                        <ItemsPresenter />
                                    </ScrollViewer>
                                </Grid>
                            </Border>
						</Border>
					</Grid>
				</Border>
			</Popup>


			<Rectangle x:Name="DisabledVisual" Visibility="Collapsed"  
					Fill="{StaticResource DisabledBrush}" />
		</Grid>
		
		
		
	</ControlTemplate>
	<!-- EditableComboBoxTemplate -->
	<ControlTemplate x:Key="EditableComboBox" TargetType="telerikRibbonBar:RadRibbonComboBox">
		<Grid>
            
			<vsm:VisualStateManager.VisualStateGroups>
				<vsm:VisualStateGroup x:Name="CommonStates">
					<vsm:VisualState x:Name="Normal" />
					<vsm:VisualState x:Name="MouseOver">
						<Storyboard>
							<ObjectAnimationUsingKeyFrames Storyboard.TargetName="ContentPresenterBorder" Storyboard.TargetProperty="Background">
								<DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource RibbonComboBoxHotBackgroundBrush}" />
							</ObjectAnimationUsingKeyFrames>
							<ObjectAnimationUsingKeyFrames Storyboard.TargetName="ContentPresenterBorder" Storyboard.TargetProperty="BorderBrush">
								<DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource RibbonComboBoxHotBorderBrush}" />
							</ObjectAnimationUsingKeyFrames>
							<ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_DropDownButton" Storyboard.TargetProperty="IsEnabled">
								<DiscreteObjectKeyFrame KeyTime="0" Value="True" />
							</ObjectAnimationUsingKeyFrames>
							<ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_DropDownButton" Storyboard.TargetProperty="Background">
								<DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource RibbonComboBoxButtonBackgroundBrush}" />
							</ObjectAnimationUsingKeyFrames>
							<ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_DropDownButton" Storyboard.TargetProperty="BorderBrush">
								<DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource RibbonComboBoxBorderBrush}" />
							</ObjectAnimationUsingKeyFrames>
						</Storyboard>
					</vsm:VisualState>
                    <vsm:VisualState x:Name="Disabled" >
                        <Storyboard Duration="0">
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DisabledVisual"
									Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </vsm:VisualState>
                </vsm:VisualStateGroup>
                <vsm:VisualStateGroup x:Name="WatermarkStates">
                    <vsm:VisualState x:Name="WatermarkVisible">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Watermark" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </vsm:VisualState>
                    <vsm:VisualState x:Name="WatermarkInvisible" />
                </vsm:VisualStateGroup>
            </vsm:VisualStateManager.VisualStateGroups>

			<Border x:Name="ContentPresenterBorder" Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}">
				<Grid>
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="*" />
						<ColumnDefinition Width="Auto" />
					</Grid.ColumnDefinitions>
					<telerik:PickerTextBox Grid.Column="0" Name="PART_EditableTextBox"
								Margin="{TemplateBinding Padding}"
								HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
								VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
								IsReadOnly="{TemplateBinding IsReadOnly}"/>

					<telerik:PickerToggleButton MinWidth="14" Margin="0,-1,-1,-1" x:Name="PART_DropDownButton" Grid.Column="1"
								Style="{StaticResource ToogleButtonStyle}"
								IsChecked="{TemplateBinding IsDropDownOpen}"
								IsEnabled="{TemplateBinding IsEnabled}" />

                    <TextBlock x:Name="Watermark" Text="{TemplateBinding EmptyText}" Opacity=".5" IsHitTestVisible="False" 
					   VerticalAlignment="Center" HorizontalAlignment="Left" Margin="5,0,0,0" Visibility="Collapsed" />
                </Grid>
			</Border>

			<Popup x:Name="PART_Popup" 
                    >
				<Border x:Name="DropDownBorder"
						HorizontalAlignment="Stretch"
						MinWidth="{TemplateBinding MinDropDownWidth}"
						MaxHeight="{TemplateBinding MaxDropDownHeight}">
					<Grid>
						<Border Background="{StaticResource RibbonPopupBackgroundBrush}" 
								BorderThickness="1"  
                                BorderBrush="{StaticResource RibbonPopupOuterBorderBrush}" >
							<Border BorderBrush="{StaticResource RibbonPopupInnerBorderBrush}" BorderThickness="1" >
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <telerik:RadButton x:Name="PART_ClearButton" Grid.Row="0"
								            Margin="-1 -1 -1 0"
								            Visibility="{TemplateBinding ClearSelectionButtonVisibility}"
								            Content="{TemplateBinding ClearSelectionButtonContent}"
								            telerik:StyleManager.Theme="{StaticResource Theme}" />

                                    <ScrollViewer x:Name="PART_ScrollViewer" Padding="0 1" Grid.Row="1"
											VerticalScrollBarVisibility="Auto" BorderThickness="0"	
                                            telerik:StyleManager.Theme="{StaticResource Theme}" 
                                            
                                            telerik:ScrollViewerExtensions.EnableMouseWheel="True"
                                            
                                             >

                                        <ItemsPresenter />
                                    </ScrollViewer>
                                </Grid>
                            </Border>
						</Border>
					</Grid>
				</Border>
			</Popup>

			<Rectangle x:Name="DisabledVisual" Visibility="Collapsed" Fill="{StaticResource DisabledBrush}" />
		</Grid>
		
	</ControlTemplate>
	<!-- RadComboBox -->
	<Style TargetType="telerikRibbonBar:RadRibbonComboBox"
            >
		<Setter Property="Background" Value="{StaticResource RibbonComboBoxBackgroundBrush}" />
		<Setter Property="BorderBrush" Value="{StaticResource RibbonComboBoxBorderBrush}" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="FontSize" Value="11" />
		<Setter Property="Foreground" Value="{StaticResource RibbonComboBoxForegroundBrush}" />
		<Setter Property="Padding" Value="3,2,3,2" />
        <Setter Property="ClearSelectionButtonContent" Value="Clear" />
        
        <Setter Property="TabNavigation" Value="Once" />
        <Setter Property="UseLayoutRounding" Value="True" />
        
        
        <Setter Property="Template" Value="{StaticResource NonEditableComboBox}" />
		<Setter Property="EditableTemplate" Value="{StaticResource EditableComboBox}" />
		<Setter Property="NonEditableTemplate" Value="{StaticResource NonEditableComboBox}" />
		<Setter Property="MinHeight" Value="22" />
		<Setter Property="HorizontalContentAlignment" Value="Left" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
        
		<Setter Property="telerikAnimation:AnimationManager.AnimationSelector">
			<Setter.Value>
				<telerikAnimation:AnimationSelector>
					<telerikAnimation:AnimationGroup AnimationName="Expand">
						<telerikAnimation:SlideAnimation Direction="In" SlideMode="Top" TargetElementName="DropDownBorder" />
					</telerikAnimation:AnimationGroup>
					<telerikAnimation:AnimationGroup AnimationName="Collapse">
						<telerikAnimation:SlideAnimation Direction="Out" SlideMode="Top" TargetElementName="DropDownBorder" />
					</telerikAnimation:AnimationGroup>
				</telerikAnimation:AnimationSelector>
			</Setter.Value>
		</Setter>
        
	</Style>
	<!-- RadComboBoxItem -->
	<Style TargetType="telerikRibbonBar:RadRibbonComboBoxItem"
            >
		<Setter Property="MinHeight" Value="17" />
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="Foreground" Value="#00156e" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonBar:RadRibbonComboBoxItem">
					<Grid x:Name="LayoutRoot" MinHeight="{TemplateBinding MinHeight}">

						<vsm:VisualStateManager.VisualStateGroups>
							<vsm:VisualStateGroup x:Name="CommonStates">
								<vsm:VisualState x:Name="Normal" />
								<vsm:VisualState x:Name="MouseOver">
									<Storyboard Duration="0">
										<ObjectAnimationUsingKeyFrames Storyboard.TargetName="Bd"
												Storyboard.TargetProperty="BorderBrush">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource ComboboxItemHotOuterBorderBrush}" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames Storyboard.TargetName="InnerBd"
												Storyboard.TargetProperty="BorderBrush">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource ComboboxItemHotInnerBorderBrush}" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames Storyboard.TargetName="InnerBd"
												Storyboard.TargetProperty="Background">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource ComboboxItemHotBackgroundBrush}" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
								<vsm:VisualState x:Name="Disabled">
									<Storyboard Duration="0">
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="DisabledVisual"
												Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
						</vsm:VisualStateManager.VisualStateGroups>


						<Border x:Name="Bd" BorderBrush="{TemplateBinding BorderBrush}"
									BorderThickness="{TemplateBinding BorderThickness}"
									CornerRadius="{StaticResource ComboboxItemCornerRadius}" >

							<Border x:Name="InnerBd" BorderBrush="{TemplateBinding BorderBrush}"
									BorderThickness="1"
									Background="{TemplateBinding Background}" CornerRadius="{StaticResource ComboboxItemInnerCornerRadius}" >

								<ContentPresenter Margin="2,0,0,0" VerticalAlignment="Center" />
							</Border>
						</Border>

						<Rectangle x:Name="DisabledVisual" Visibility="Collapsed"
								Fill="{StaticResource DisabledBrush}" />
					</Grid>
					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	
	
	

	<Style TargetType="telerikRibbonBar:RadRibbonDropDownButton"
		
		>
		<Setter Property="Padding" Value="3" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="HorizontalAlignment" Value="Left" />
		<Setter Property="HorizontalContentAlignment" Value="Center" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="DropDownButtonPosition" Value="Bottom" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonBar:RadRibbonDropDownButton">
					<Grid>
						
						<vsm:VisualStateManager.VisualStateGroups>
							<vsm:VisualStateGroup x:Name="CommonStates">
								<vsm:VisualState x:Name="Normal" />
								<vsm:VisualState x:Name="MouseOver">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="BorderBrush"
												Storyboard.TargetName="backgroundDecorator">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadRibbonBarButtonOverBorder}" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="Background"
												Storyboard.TargetName="backgroundDecorator">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadRibbonBarButtonOverBackground}" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="BorderBrush"
												Storyboard.TargetName="backgroundDecoratorInner">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadRibbonBarButtonOverBorderInner}" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
								<vsm:VisualState x:Name="Pressed">
									<Storyboard>
										<DoubleAnimation Duration="0" To="1"
												Storyboard.TargetProperty="Opacity"
												Storyboard.TargetName="PressedDecorator" />
									</Storyboard>
								</vsm:VisualState>
								<vsm:VisualState x:Name="Disabled">
									<Storyboard Duration="0">
                                        <DoubleAnimation Duration="0" To="0.5"
												Storyboard.TargetName="ContentGrid"
												Storyboard.TargetProperty="Opacity" />
                                    </Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="FocusState">
								<vsm:VisualState x:Name="Focused" />
								<vsm:VisualState x:Name="Unfocused" />
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="TextAlignmentState">
								<vsm:VisualState x:Name="TextAtBottom" />
								<vsm:VisualState x:Name="TextAtRight">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="(Grid.Row)">
											<DiscreteObjectKeyFrame KeyTime="0" Value="0" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="(Grid.Column)">
											<DiscreteObjectKeyFrame KeyTime="0" Value="1" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="Margin">
											<DiscreteObjectKeyFrame KeyTime="0" Value="4,0,3,0" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="VerticalAlignment">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Center" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="TextAlignment">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Left" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow2"
												Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
								<vsm:VisualState x:Name="TextHide">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow2"
												Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="IsOpenState">
								<vsm:VisualState x:Name="Closed" />
								<vsm:VisualState x:Name="Opened">
									<Storyboard>
										<DoubleAnimation Duration="0" To="1"
												Storyboard.TargetProperty="Opacity"
												Storyboard.TargetName="PressedDecorator" />
									</Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="DropDownButtonPositionState">
								<vsm:VisualState x:Name="DropDownButtonAtTop" />
								<vsm:VisualState x:Name="DropDownButtonAtLeft" />
								<vsm:VisualState x:Name="DropDownButtonAtBottom" />
								<vsm:VisualState x:Name="DropDownButtonAtRight">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="DropDownPanel"
												Storyboard.TargetProperty="(Grid.Column)">
											<DiscreteObjectKeyFrame KeyTime="0" Value="2" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="DropDownPanel"
												Storyboard.TargetProperty="(Grid.Row)">
											<DiscreteObjectKeyFrame KeyTime="0" Value="0" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="DropDownPanel"
												Storyboard.TargetProperty="Margin">
											<DiscreteObjectKeyFrame KeyTime="0" Value="2,0,0,0" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow2"
												Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="DropDownPlacementState">
								<vsm:VisualState x:Name="PlacementBottom"/>
								<vsm:VisualState x:Name="PlacementTop"/>
								<vsm:VisualState x:Name="PlacementLeft"/>
								<vsm:VisualState x:Name="PlacementRight"/>
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="DropDownContentState">
								<vsm:VisualState x:Name="HasDropDownContent" />
								<vsm:VisualState x:Name="WithoutDropDownContent">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="Visibility"
												Storyboard.TargetName="DropDownIndicator">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="DropShadowVisibilityState">
								<vsm:VisualState x:Name="HasShadow" />
								<vsm:VisualState x:Name="WithoutShadow" />
							</vsm:VisualStateGroup>
						</vsm:VisualStateManager.VisualStateGroups>
						

						<Border x:Name="backgroundDecorator"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}"
								Background="{TemplateBinding Background}"
								CornerRadius="2">
							<Border x:Name="backgroundDecoratorInner"
									BorderBrush="Transparent"
									BorderThickness="1"/>
						</Border>

						<Border x:Name="PressedDecorator" Opacity="0"
								BorderThickness="1"
								BorderBrush="{StaticResource RadRibbonBarDropDownButtonPressedBorder}"
								Background="{StaticResource RadRibbonBarDropDownButtonPressedBackground}"
								CornerRadius="2">
							<Border x:Name="PressedDecoratorInner"
									BorderBrush="{StaticResource RadRibbonBarDropDownButtonPressedBorderInner}"
									BorderThickness="1"
									/>
						</Border>

                        <Grid Margin="{TemplateBinding Padding}" x:Name="ContentGrid">
							<Grid.RowDefinitions>
								<RowDefinition Height="Auto" />
								<RowDefinition Height="*" MaxHeight="28" />
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="Auto" />
								<ColumnDefinition Width="*" />
								<ColumnDefinition Width="Auto" />
							</Grid.ColumnDefinitions>

							<ContentPresenter
								Name="ContentPresenter"
								Grid.ColumnSpan="2" Grid.RowSpan="9"
								Content="{TemplateBinding Content}"
								ContentTemplate="{TemplateBinding ContentTemplate}"
								VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
								HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
								 />

							<Image Name="Image" />

							<TextBlock Name="textRow1" Grid.Row="1" Margin="3,2,2,0"
									Text="{TemplateBinding TextRow1}" TextAlignment="Center" />

							<StackPanel Name="DropDownPanel" Grid.Row="1" Margin="2,14,0,0"
									HorizontalAlignment="Center" Orientation="Horizontal"
									MinHeight="14" MaxHeight="14" Height="14">
								<TextBlock Name="textRow2" Text="{TemplateBinding TextRow2}" />
								<Path
									Name="DropDownIndicator"
									IsHitTestVisible="False"
									VerticalAlignment="Center"
									Visibility="{TemplateBinding DropDownIndicatorVisibility}"
									Fill="{StaticResource RibbonButtonDropDownArrow}"
									Stroke="{StaticResource RibbonButtonDropDownArrowBorder}"
									Data="M 0,0 L 3.5,3.5 L 7,0"
									Margin="2,0,2,0"
									 />
							</StackPanel>
						</Grid>
						<Popup
							x:Name="DropDownPopup"
							
							>
							<Grid>
								<!--<telerikRibbonBarPrimitives:DropShadow Margin="6,6,4,4" />-->
								<Border x:Name="DropDownPopupBorder"
										BorderBrush="{StaticResource DropDownPopupBorderBrush}"
										Background="{StaticResource DropDownPopupBackgroundBrush}"
										BorderThickness="1" Padding="1" Margin="0"
										MinWidth="3"
										MinHeight="3"
										Width="{TemplateBinding DropDownWidth}"
										Height="{TemplateBinding DropDownHeight}"
										MaxWidth="{TemplateBinding DropDownMaxWidth}"
										MaxHeight="{TemplateBinding DropDownMaxHeight}">
									<ContentPresenter
										x:Name="DropDownPopupContent"
										Content="{TemplateBinding DropDownContent}"
										ContentTemplate="{TemplateBinding DropDownContentTemplate}"
										HorizontalAlignment="Stretch"
										VerticalAlignment="Stretch"
										DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}"/>
								</Border>
							</Grid>
						</Popup>

					</Grid>

					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	
	

    <Style TargetType="telerikRibbonBar:RadRibbonGallery"
        >
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="BorderBrush" Value="{StaticResource RibbonGalleryBorderBrush}" />
        <Setter Property="Background" Value="{StaticResource RibbonGalleryBackgroundBrush}" />
        <Setter Property="ViewportWidth" Value="217" />
        <Setter Property="ViewportHeight" Value="58" />
		
		<Setter Property="MoreButtonTitle" Value="More" telerik:LocalizationManager.ResourceKey="RibbonBarGalleryButtonMore"/>
		<Setter Property="UpDownButtonTitle" Value="Row " telerik:LocalizationManager.ResourceKey="RibbonBarGalleryButtonUpDown"/>
		
		
        <Setter Property="ItemWidth" Value="20"/>
        <Setter Property="ItemHeight" Value="20"/>
        <Setter Property="PopupLaunchButtonVisibility" Value="Visible" />
        <!--<Setter Property="PopupViewportHeight" Value="170" />-->
        <Setter Property="PopupViewportWidth" Value="237" />
        <Setter Property="Icon" Value="/Telerik.Windows.Controls.RibbonBar;component/RibbonGalleryCollapsedIcon.png" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <telerik:RadWrapPanel 
                         />
                    
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:RadRibbonGallery">
                    <Grid>
                        
                        <vsm:VisualStateManager.VisualStateGroups>
                            <vsm:VisualStateGroup x:Name="CollapsedState">
                                <vsm:VisualState x:Name="Expanded" />
                                <vsm:VisualState x:Name="Collapsed">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ExpandedBorder" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="CollapsedButton" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="PopupMenuItemsState">
                                <vsm:VisualState x:Name="HasPopupMenuItems" />
                                <vsm:VisualState x:Name="NoPopupMenuItems">
                                    <!--<Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PopupMenuItemsSeparator" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>-->
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="CommonStates">
                                <vsm:VisualState x:Name="Normal" />
                                <vsm:VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
                                            Storyboard.TargetName="ExpandedBorder"
												Storyboard.TargetProperty="Background">
                                            <DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RibbonGalleryOverBackgroundBrush}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                        </vsm:VisualStateManager.VisualStateGroups>
                        
                        <Border Name="ExpandedBorder" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}" CornerRadius="0,1,1,0">
                            <Grid >
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="14" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <ScrollViewer Name="ScrollViewer"  Grid.RowSpan="3" VerticalScrollBarVisibility="Hidden" IsTabStop="False" BorderThickness="0" Height="{TemplateBinding ViewportHeight}" Padding="0,1,0,1">
                                    <Border Name="InRibbonHost" Width="{TemplateBinding ViewportWidth}" HorizontalAlignment="Left" >
                                        <ItemsPresenter Name="ItemsPresenter" />
                                    </Border>
                                </ScrollViewer>
                               
                                <telerikRibbonBar:RadRibbonButton telerik:StyleManager.Theme="{StaticResource Theme}" telerikRibbonBar:ScreenTip.Title="{TemplateBinding UpDownButtonTitle}"  HorizontalAlignment="Stretch" Name="ScrollUpButton" Grid.Column="1" Grid.Row="0" IsTabStop="False" BorderBrush="{TemplateBinding BorderBrush}" Margin="0,-1,-1,0" Background="{StaticResource RibbonGalleryItemBackgroundBrush}" Padding="0" >
                                    <Path Margin="4 1 0 0"  Data="M 0,3.5 L 3.5,0 L 7,3.5" Fill="{StaticResource RibbonGroupButtonArrow}" Stroke="{StaticResource RibbonGroupButtonArrowHighlight}" />
                                </telerikRibbonBar:RadRibbonButton>
                                <telerikRibbonBar:RadRibbonButton telerik:StyleManager.Theme="{StaticResource Theme}" telerikRibbonBar:ScreenTip.Title="{TemplateBinding UpDownButtonTitle}"   HorizontalAlignment="Stretch" Name="ScrollDownButton" Grid.Column="1" Grid.Row="1" IsTabStop="False" BorderBrush="{TemplateBinding BorderBrush}" Margin="0,-1,-1,-1" Background="{StaticResource RibbonGalleryItemBackgroundBrush}" Padding="0">
                                    <Path  Margin="4 2 0 0" HorizontalAlignment="Center" VerticalAlignment="Center" Data="M 0,0 L 3.5,3.5 L 7,0" Fill="{StaticResource RibbonGroupButtonArrow}" Stroke="{StaticResource RibbonGroupButtonArrowHighlight}" />
                                </telerikRibbonBar:RadRibbonButton>
                                <telerikRibbonBar:RadRibbonDropDownButton telerik:StyleManager.Theme="{StaticResource Theme}" telerikRibbonBar:ScreenTip.Title="{TemplateBinding MoreButtonTitle}" HorizontalAlignment="Stretch"   DropDownIndicatorVisibility="Collapsed" Visibility="{TemplateBinding PopupLaunchButtonVisibility}" x:Name="PopupButton" Grid.Column="1" Grid.Row="2" BorderBrush="{TemplateBinding BorderBrush}" Margin="0,0,-1,-1" Background="{StaticResource RibbonGalleryItemBackgroundBrush}" Padding="0,0,-1,0">
                                    <Path HorizontalAlignment="Center" VerticalAlignment="Center" Margin="2,4,0,0" Data="M 0,-2.5 L 0,-1.5 L 7,-1.5 L 7,-2.5 M 0,0 L 3.5,3.5 L 7,0" Fill="{StaticResource RibbonGroupButtonArrow}" Stroke="{StaticResource RibbonGroupButtonArrowHighlight}" />
                                </telerikRibbonBar:RadRibbonDropDownButton>
                               
                            </Grid>
                        </Border>
                       
                        <telerikRibbonBar:RadRibbonDropDownButton telerik:StyleManager.Theme="{StaticResource Theme}" Name="CollapsedButton" Size="Large" LargeImage="{TemplateBinding Icon}" Text="{TemplateBinding Title}" Visibility="Collapsed">
                            <telerikRibbonBar:RadRibbonDropDownButton.DropDownContent>
                                <Grid Width="{TemplateBinding PopupViewportWidth}" >
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <ScrollViewer telerik:StyleManager.Theme="{StaticResource Theme}"
												  Grid.Row="0" 
												  IsTabStop="False" 
												  BorderThickness="0" Margin="0"  
												  Height="{TemplateBinding PopupViewportHeight}" Padding="0"
												VerticalScrollBarVisibility="Visible">
                                        <Border Name="PopupHost" HorizontalAlignment="Left" Margin="0"  >
                                        </Border>
                                    </ScrollViewer>

                                    <telerikNavigation:RadContextMenu telerik:StyleManager.Theme="{StaticResource Theme}" 
                                         HorizontalContentAlignment="Stretch" x:Name="menu" BorderThickness="0,1,0,0" Grid.Row="1"  ItemsSource="{TemplateBinding PopupMenuItems}" />
                                   
                                </Grid>
                            </telerikRibbonBar:RadRibbonDropDownButton.DropDownContent>
                        </telerikRibbonBar:RadRibbonDropDownButton>
                        
                    </Grid>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
	

	<Style TargetType="telerikRibbonBar:RadRibbonDropDownButton" x:Key="DropDownButtonGroup">
		<Setter Property="Padding" Value="3" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="Background" Value="{StaticResource RadRibbonGroupNormalBackground}" />
		<Setter Property="BorderBrush" Value="{StaticResource RadRibbonGroupNormalBorder}" />
		<Setter Property="HorizontalContentAlignment" Value="Center" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Setter Property="VerticalAlignment" Value="Stretch" />
		<Setter Property="MaxHeight" Value="999" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonBar:RadRibbonDropDownButton">
                    <Grid >
                       
                        <vsm:VisualStateManager.VisualStateGroups>
							<vsm:VisualStateGroup x:Name="CommonStates">
								<vsm:VisualState x:Name="Normal" />
								<vsm:VisualState x:Name="MouseOver">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="BorderBrush"
												Storyboard.TargetName="buttonSurface">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadRibbonGroupOverBorder}" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="Background"
												Storyboard.TargetName="buttonSurface">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadRibbonGroupOverBackground}" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="BorderBrush"
												Storyboard.TargetName="buttonSurfaceInner">
											<DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadRibbonGroupOverBorderInner}" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
								<vsm:VisualState x:Name="Pressed">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="Opacity"
												Storyboard.TargetName="mouseOverIndicator">
											<DiscreteObjectKeyFrame KeyTime="0" Value="1" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
								<vsm:VisualState x:Name="Disabled">
									<Storyboard Duration="0">
                                        <DoubleAnimation Duration="0" To="0.5"
												Storyboard.TargetName="ContentGrid"
												Storyboard.TargetProperty="Opacity" />
                                    </Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="FocusStates">
								<vsm:VisualState x:Name="Focused" />
								<vsm:VisualState x:Name="Unfocused" />
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="TextAlignment">
								<vsm:VisualState x:Name="TextAtBottom" />
								<vsm:VisualState x:Name="TextAtRight">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="(Grid.Row)">
											<DiscreteObjectKeyFrame KeyTime="0" Value="0" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="(Grid.Column)">
											<DiscreteObjectKeyFrame KeyTime="0" Value="1" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="Margin">
											<DiscreteObjectKeyFrame KeyTime="0" Value="4,0,1,0" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="VerticalAlignment">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Center" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="TextAlignment">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Left" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow2"
												Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
								<vsm:VisualState x:Name="TextHide">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1"
												Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow2"
												Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="IsOpenState">
								<vsm:VisualState x:Name="Closed" />
								<vsm:VisualState x:Name="Opened">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="Opacity"
												Storyboard.TargetName="mouseOverIndicator">
											<DiscreteObjectKeyFrame KeyTime="0" Value="1" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="SplitState">
								<vsm:VisualState x:Name="DropDownButtonAtTop" />
								<vsm:VisualState x:Name="DropDownButtonAtLeft" />
								<vsm:VisualState x:Name="DropDownButtonAtBottom" />
								<vsm:VisualState x:Name="DropDownButtonAtRight">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="DropDownPanel"
												Storyboard.TargetProperty="(Grid.Column)">
											<DiscreteObjectKeyFrame KeyTime="0" Value="2" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="DropDownPanel"
												Storyboard.TargetProperty="(Grid.Row)">
											<DiscreteObjectKeyFrame KeyTime="0" Value="0" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="DropDownPanel"
												Storyboard.TargetProperty="Margin">
											<DiscreteObjectKeyFrame KeyTime="0" Value="0,0,0,0" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow2"
												Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="DropDownContentStates">
								<vsm:VisualState x:Name="HasDropDownContent" />
								<vsm:VisualState x:Name="WithoutDropDownContent">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="Visibility"
												Storyboard.TargetName="DropDownIndicator">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="DropShadowVisibilityStates">
								<vsm:VisualState x:Name="HasShadow" />
								<vsm:VisualState x:Name="WithoutShadow" />
							</vsm:VisualStateGroup>
						</vsm:VisualStateManager.VisualStateGroups>
						
                        <Border x:Name="buttonSurface"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}"
								Background="{TemplateBinding Background}"
								Margin="0,0,0,1"
								CornerRadius="3">
							<Border x:Name="buttonSurfaceInner"
									BorderBrush="{StaticResource RadRibbonGroupNormalBorderInner}"
									BorderThickness="1 0 0 1" CornerRadius="2"/>
						</Border>

						<Border x:Name="mouseOverIndicator" Opacity="0"
								BorderThickness="1"
								BorderBrush="{StaticResource RadRibbonGroupPressedBorder}"
								Background="{StaticResource RadRibbonGroupPressedBackground}"
								CornerRadius="3">
							<Border x:Name="mouseOverIndicatorInner"
									BorderBrush="{StaticResource RadRibbonGroupPressedBorderInner}"
									BorderThickness="1 0 0 1"
									Background="Transparent"
									CornerRadius="2" />

						</Border>


                        <Grid Margin="{TemplateBinding Padding}" x:Name="ContentGrid">
							<Grid.RowDefinitions>
								<RowDefinition Height="Auto" />
								<RowDefinition Height="*" MaxHeight="28" />
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="Auto" />
								<ColumnDefinition Width="*" />
								<ColumnDefinition Width="Auto" />
							</Grid.ColumnDefinitions>

							<ContentPresenter Grid.ColumnSpan="3" Grid.RowSpan="2"
                                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
									VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
									Content="{TemplateBinding Content}"
									ContentTemplate="{TemplateBinding ContentTemplate}"/>

							<Image Name="Image" />

							<TextBlock Name="textRow1" Grid.Row="1" Margin="0,2,0,0"
									Text="{TemplateBinding TextRow1}" TextAlignment="Center" />

							<StackPanel Name="DropDownPanel" Grid.Row="1" Margin="0,14,0,0"
									HorizontalAlignment="Center" Orientation="Horizontal"
									MinHeight="14" MaxHeight="14" Height="14">
								<TextBlock Name="textRow2" Text="{TemplateBinding TextRow2}" />
								<Path Name="DropDownIndicator" VerticalAlignment="Center"
										Data="M 0,0 L 3.5,3.5 L 7,0" Margin="2,0,0,0"
										Visibility="{TemplateBinding DropDownIndicatorVisibility}"
										Fill="{StaticResource RibbonButtonDropDownArrow}"
										Stroke="{StaticResource RibbonButtonDropDownArrowBorder}" />
							</StackPanel>
						</Grid>
                        
                            
                            <Popup x:Name="DropDownPopup">
                                
                         
							<Grid>
								<!--<telerikRibbonBarPrimitives:DropShadow Margin="6,6,1,1" />-->
								
								<Border x:Name="DropDownPopupBorder"
									BorderBrush="Transparent"
									Background="Transparent"
									MinWidth="33" MinHeight="33">
									<ContentControl x:Name="DropDownPopupContent"
										Content="{TemplateBinding DropDownContent}"
										HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
								</Border>
							</Grid>
						</Popup>

						<Rectangle x:Name="DisabledIndicator" Visibility="Collapsed" Fill="#99FFFFFF" />
					</Grid>
						
					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style TargetType="telerikRibbonBar:RadRibbonGroup"
            >
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="Foreground" Value="{StaticResource RibbonForegroundBrush}" />
		<Setter Property="SeparatorStyle">
			<Setter.Value>
				<Style TargetType="telerikRibbonBar:Separator">
					<Setter Property="Background" Value="{StaticResource RibbonGroupSeparatorLightBrush}" />
					<Setter Property="BorderThickness" Value="0,0,1,0" />
					<Setter Property="BorderBrush" Value="{StaticResource RibbonGroupSeparatorDarkBrush}" />
					<Setter Property="IsTabStop" Value="False" />
					<Setter Property="Width" Value="2" />
					<Setter Property="Margin" Value="0,5,0,6" />
					<Setter Property="Template">
						<Setter.Value>
							<ControlTemplate TargetType="telerikRibbonBar:Separator">
								<Border BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}" />
							</ControlTemplate>
						</Setter.Value>
					</Setter>
				</Style>
			</Setter.Value>
		</Setter>
		<Setter Property="ItemsPanel">
			<Setter.Value>
				<ItemsPanelTemplate>
					<telerikRibbonBarPrimitives:StripPanel />
				</ItemsPanelTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonBar:RadRibbonGroup">
					<Grid  >
                        
                        <vsm:VisualStateManager.VisualStateGroups>
							<vsm:VisualStateGroup x:Name="SizeState">
								<vsm:VisualState x:Name="Expanded" />
								<vsm:VisualState x:Name="Collapsed">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames Storyboard.TargetName="CollapsedDisplay" Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames Storyboard.TargetName="CollapsedButton" Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="CollapsedGroup">
								<vsm:VisualState x:Name="CollapsedGroupNormal" >
								</vsm:VisualState>
								<vsm:VisualState x:Name="CollapsedGroupPushed" >
									<Storyboard>
										<ObjectAnimationUsingKeyFrames Storyboard.TargetName="GroupImage" Storyboard.TargetProperty="Background">
											<DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource RibbonGroupPushedButtonBackgroundBrush}" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
									
							</vsm:VisualStateGroup>
							
						</vsm:VisualStateManager.VisualStateGroups>
                        
                        <telerikRibbonBarPrimitives:GroupChrome telerik:StyleManager.Theme="{StaticResource Theme}"  Name="ExpandedChrome" Header="{TemplateBinding Header}" DialogLauncherVisibility="{TemplateBinding DialogLauncherVisibility}">
							<ItemsPresenter Name="ItemsPresenter" />
						</telerikRibbonBarPrimitives:GroupChrome>
                        <!---->
                        <telerikRibbonBar:RadRibbonDropDownButton 
							
							Visibility="Collapsed"
							Style="{StaticResource DropDownButtonGroup}" DropDownIndicatorVisibility="Collapsed" Name="CollapsedButton" DropDownButtonPosition="Right" HorizontalAlignment="Stretch">
							<telerikRibbonBar:RadRibbonDropDownButton.DropDownContent>
								<Border CornerRadius="2" Background="{StaticResource RibbonTabBackgroundBrush}">
									<telerikRibbonBarPrimitives:GroupChrome telerik:StyleManager.Theme="{StaticResource Theme}" Name="CollapsedChrome"/>
								</Border>
							</telerikRibbonBar:RadRibbonDropDownButton.DropDownContent>
						</telerikRibbonBar:RadRibbonDropDownButton>
                       
                        
                        <Grid Name="CollapsedDisplay"
							
							
							Visibility="Collapsed"
							 
							Margin="2" VerticalAlignment="Top" IsHitTestVisible="False">
							<Grid.RowDefinitions>
								<RowDefinition Height="Auto" />
								<RowDefinition Height="Auto" />
								<RowDefinition Height="Auto" />
							</Grid.RowDefinitions>
							<Border x:Name="GroupImage" BorderBrush="{StaticResource RibbonGroupButtonBorderBrush}" BorderThickness="1" CornerRadius="4" Width="31" Height="31" Background="{StaticResource RibbonGroupButtonBackgroundBrush}" Margin="3">
								<Grid>
									<Border VerticalAlignment="Bottom" Height="7" CornerRadius="0,0,3,3" Background="{StaticResource RibbonGroupButtonBottomBrush}" />
									<Image VerticalAlignment="Center" HorizontalAlignment="Center" Margin="0,0,0,7" Source="{TemplateBinding Icon}" Width="16" Height="16" />
								</Grid>
							</Border>
							<ContentControl Content="{TemplateBinding Header}" Foreground="{TemplateBinding Foreground}" Grid.Row="1" Margin="1,2,1,3" HorizontalContentAlignment="Center" />
							<Path Grid.Row="2" VerticalAlignment="Top" HorizontalAlignment="Center" Fill="{StaticResource RibbonGroupButtonArrow}" Stroke="{StaticResource RibbonGroupButtonArrowHighlight}" Data="M 0,0 L 3.5,3.5 L 7,0" />
						</Grid>
					</Grid>
                    
                </ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	
	

    <Style TargetType="telerikRibbonBar:RadRibbonRadioButton"
		
        >
        <Setter Property="Padding" Value="3" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:RadRibbonRadioButton">
                    <Grid>
                        
                        <vsm:VisualStateManager.VisualStateGroups>
                            <vsm:VisualStateGroup x:Name="CommonStates">
                                <vsm:VisualState x:Name="Normal" />
                                <vsm:VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="BorderBrush"
												Storyboard.TargetName="buttonSurface">
                                            <DiscreteObjectKeyFrame KeyTime="0"
													Value="{StaticResource RadRibbonBarButtonOverBorder}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="Background"
