		<Setter Property="Background" Value="{StaticResource RibbonPopupBackgroundBrush}" />
		<Setter Property="BorderBrush" Value="{StaticResource ApplicationMenuOuterBorderBrush}" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="Foreground" Value="{StaticResource RibbonPopupForegroundBrush}" />
		<Setter Property="ItemContainerStyleSelector" Value="{StaticResource ApplicationMenuContainerStyleSelector}" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonBar:ApplicationMenu">
