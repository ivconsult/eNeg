    <Style TargetType="telerikRibbonBar:RadGalleryItem"
            >
        <Setter Property="Padding" Value="2" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:RadGalleryItem">
                    <Grid  >
                        
                        <vsm:VisualStateManager.VisualStateGroups>
                            <vsm:VisualStateGroup x:Name="CommonStates">
                                <vsm:VisualState x:Name="Normal" />
                                <vsm:VisualState x:Name="MouseOver">
                                    <Storyboard>
										<DoubleAnimation Duration="0" To="1"
												Storyboard.TargetProperty="opacity"
												Storyboard.TargetName="mouseOverIndicator" />
                                    </Storyboard>
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="SelectedMouseOver">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0" To="1"
												Storyboard.TargetProperty="opacity"
												Storyboard.TargetName="mouseOverIndicator" />
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="SelectionStates">
                                <vsm:VisualState x:Name="Unselected" />
                                <vsm:VisualState x:Name="Selected">
                                    <Storyboard>
                                       <DoubleAnimation Duration="0" To="1"
												Storyboard.TargetProperty="opacity"
												Storyboard.TargetName="SelectedIndicator" />
                                    </Storyboard>
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                            <vsm:VisualStateGroup x:Name="FocusStates">
                                <vsm:VisualState x:Name="Focused">
                                </vsm:VisualState>
                                <vsm:VisualState x:Name="Unfocused">
                                </vsm:VisualState>
                            </vsm:VisualStateGroup>
                        </vsm:VisualStateManager.VisualStateGroups>
                        
                        <Border x:Name="buttonSurface" CornerRadius="1.5"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}"
								Background="{TemplateBinding Background}" />
						
						<!-- Over -->
						<Border x:Name="mouseOverIndicator" CornerRadius="1.5" Opacity="0"
								BorderBrush="{StaticResource RadGalleryItemOver_border}"
								Background="{StaticResource RadGalleryItemOver_background}"
								BorderThickness="{TemplateBinding BorderThickness}">
							<Border BorderThickness="1" BorderBrush="{StaticResource RadGalleryItemOver_Innerborder}" />
						</Border>
						
						<!-- Selected -->
						<Border x:Name="SelectedIndicator" CornerRadius="1.5" Opacity="0"
								BorderBrush="{StaticResource RadGalleryItemPressed_border}"
								Background="{StaticResource RadGalleryItemPressed_background}"
								BorderThickness="{TemplateBinding BorderThickness}">
							<Border BorderThickness="1"
									BorderBrush="{StaticResource RadGalleryItemPressed_Innerborder}" />
						</Border>

						<Grid >
                            <!--<Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="*" MaxHeight="28" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>-->

                            <ContentPresenter Content="{TemplateBinding Content}"  	HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>

                            <Image Name="Image" Margin="{TemplateBinding Padding}" Stretch="None"  Source="{TemplateBinding Image}"  HorizontalAlignment="{TemplateBinding HorizontalAlignment}" VerticalAlignment="{TemplateBinding VerticalAlignment}"/>

                        </Grid>

                        <Rectangle x:Name="DisabledIndicator" Visibility="Collapsed" Fill="#99FFFFFF" />
                    </Grid>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    
	

