								<vsm:VisualState x:Name="Text1Hide">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetName="textRow1" Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
