	<Style TargetType="telerikRibbonBarPrimitives:GroupChrome"
         >
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="Foreground" Value="{StaticResource RibbonForegroundBrush}" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonBarPrimitives:GroupChrome">
					<Grid Background="Transparent"   >
						
						<vsm:VisualStateManager.VisualStateGroups>
							<vsm:VisualStateGroup x:Name="SizeState">
								<vsm:VisualState x:Name="Expanded" />
								<vsm:VisualState x:Name="Collapsed">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames Storyboard.TargetName="Tray" Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="InteractionState">
								<vsm:VisualState x:Name="Normal">
									<Storyboard>
										<ColorAnimation Storyboard.TargetName="InnerBorder1" Storyboard.TargetProperty="Color" To="{StaticResource RibbonGroupNormalInnerBorderColor1}" Duration="00:00:00.3" />
										<ColorAnimation Storyboard.TargetName="InnerBorder2" Storyboard.TargetProperty="Color" To="{StaticResource RibbonGroupNormalInnerBorderColor2}" Duration="00:00:00.3" />
										<ColorAnimation Storyboard.TargetName="OuterBorder1" Storyboard.TargetProperty="Color" To="{StaticResource RibbonGroupNormalOuterBorderColor1}" Duration="00:00:00.3" />
										<ColorAnimation Storyboard.TargetName="OuterBorder2" Storyboard.TargetProperty="Color" To="{StaticResource RibbonGroupNormalOuterBorderColor2}" Duration="00:00:00.3" />
										<ColorAnimation Storyboard.TargetName="Tray1" Storyboard.TargetProperty="Color" To="{StaticResource RibbonGroupNormalTrayBackgroundColor1}" Duration="00:00:00.3" />
										<ColorAnimation Storyboard.TargetName="Tray2" Storyboard.TargetProperty="Color" To="{StaticResource RibbonGroupNormalTrayBackgroundColor2}" Duration="00:00:00.3" />
										<DoubleAnimation Storyboard.TargetName="BackgroundBrush" Storyboard.TargetProperty="Opacity" To="0" Duration="00:00:00.3" />
									</Storyboard>
								</vsm:VisualState>
								<vsm:VisualState x:Name="MouseOver">
									<Storyboard>
										<ColorAnimation Storyboard.TargetName="InnerBorder1" Storyboard.TargetProperty="Color" To="{StaticResource RibbonGroupHotInnerBorderColor1}" Duration="00:00:00.3" />
										<ColorAnimation Storyboard.TargetName="InnerBorder2" Storyboard.TargetProperty="Color" To="{StaticResource RibbonGroupHotInnerBorderColor2}" Duration="00:00:00.3" />
										<ColorAnimation Storyboard.TargetName="OuterBorder1" Storyboard.TargetProperty="Color" To="{StaticResource RibbonGroupHotOuterBorderColor1}" Duration="00:00:00.3" />
										<ColorAnimation Storyboard.TargetName="OuterBorder2" Storyboard.TargetProperty="Color" To="{StaticResource RibbonGroupHotOuterBorderColor2}" Duration="00:00:00.3" />
										<ColorAnimation Storyboard.TargetName="Tray1" Storyboard.TargetProperty="Color" To="{StaticResource RibbonGroupHotTrayBackgroundColor1}" Duration="00:00:00.3" />
										<ColorAnimation Storyboard.TargetName="Tray2" Storyboard.TargetProperty="Color" To="{StaticResource RibbonGroupHotTrayBackgroundColor2}" Duration="00:00:00.3" />
										<DoubleAnimation Storyboard.TargetName="BackgroundBrush" Storyboard.TargetProperty="Opacity" To="1" Duration="00:00:00.3" />
										<ObjectAnimationUsingKeyFrames Storyboard.TargetName="OuterBorder" Storyboard.TargetProperty="Margin">
											<DiscreteObjectKeyFrame KeyTime="0" Value="0,0,0,1" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Margin">
											<DiscreteObjectKeyFrame KeyTime="0" Value="1,1,1,2" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="(Canvas.ZIndex)">
											<DiscreteObjectKeyFrame KeyTime="0" Value="1" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
								<vsm:VisualState x:Name="Pushed">
									<Storyboard>
										<ColorAnimation Storyboard.TargetName="InnerBorder1" Storyboard.TargetProperty="Color" To="{StaticResource RibbonGroupPushedInnerBorderColor1}" Duration="0" />
										<ColorAnimation Storyboard.TargetName="InnerBorder2" Storyboard.TargetProperty="Color" To="{StaticResource RibbonGroupPushedInnerBorderColor2}" Duration="0" />
										<ColorAnimation Storyboard.TargetName="OuterBorder1" Storyboard.TargetProperty="Color" To="{StaticResource RibbonGroupPushedOuterBorderColor1}" Duration="0" />
										<ColorAnimation Storyboard.TargetName="OuterBorder2" Storyboard.TargetProperty="Color" To="{StaticResource RibbonGroupPushedOuterBorderColor2}" Duration="0" />
										<DoubleAnimation Storyboard.TargetName="BackgroundBrush" Storyboard.TargetProperty="Opacity" To="1" Duration="0" />
										<ObjectAnimationUsingKeyFrames Storyboard.TargetName="OuterBorder" Storyboard.TargetProperty="Background">
											<DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource RibbonGroupPushedBackgroundBrush}" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
						</vsm:VisualStateManager.VisualStateGroups>
						
						<Border Name="InnerBorder" Margin="1,1,0,0" BorderThickness="1" CornerRadius="1">
							<Border.BorderBrush>
								<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
									<GradientStop x:Name="InnerBorder1" Offset="0" Color="{StaticResource RibbonGroupNormalInnerBorderColor1}" />
									<GradientStop x:Name="InnerBorder2" Offset="1" Color="{StaticResource RibbonGroupNormalInnerBorderColor2}" />
								</LinearGradientBrush>
							</Border.BorderBrush>
						</Border>
						<Border Name="OuterBorder" Margin="0,0,1,1" BorderThickness="1" CornerRadius="2">
							<Border.Background>
								<LinearGradientBrush x:Name="BackgroundBrush" StartPoint="0,0" EndPoint="0,1" Opacity="0">
									<GradientStop Offset="0" Color="{StaticResource RibbonGroupBackground1}" />
									<GradientStop Offset="0.17" Color="{StaticResource RibbonGroupBackground2}" />
									<GradientStop Offset="0.17" Color="{StaticResource RibbonGroupBackground3}" />
									<GradientStop Offset="1" Color="{StaticResource RibbonGroupBackground4}" />
								</LinearGradientBrush>
							</Border.Background>
							<Border.BorderBrush>
								<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
									<GradientStop x:Name="OuterBorder1" Offset="0" Color="{StaticResource RibbonGroupNormalOuterBorderColor1}" />
									<GradientStop x:Name="OuterBorder2" Offset="1" Color="{StaticResource RibbonGroupNormalOuterBorderColor2}" />
								</LinearGradientBrush>
							</Border.BorderBrush>
						</Border>
						<Border Name="Tray" Height="15" VerticalAlignment="Bottom" Margin="1,0,2,2">
							<Border.Background>
								<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
									<GradientStop x:Name="Tray1" Offset="0.5" Color="{StaticResource RibbonGroupNormalTrayBackgroundColor1}" />
									<GradientStop x:Name="Tray2" Offset="1" Color="{StaticResource RibbonGroupNormalTrayBackgroundColor2}" />
								</LinearGradientBrush>
							</Border.Background>
							<Grid>
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="*" />
									<ColumnDefinition Width="Auto" />
								</Grid.ColumnDefinitions>
								<ContentControl Content="{TemplateBinding Header}" Margin="3,0,3,0" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="{StaticResource RibbonGroupHeaderBrush}" />

								<telerikRibbonBar:RadRibbonButton telerik:StyleManager.Theme="{StaticResource Theme}" Visibility="{TemplateBinding DialogLauncherVisibility}" Name="DialogLauncher" Grid.Column="1" IsAutoSize="False" VerticalAlignment="Top" Padding="4,3,3,3" TabIndex="1" >
									<telerikRibbonBar:RadRibbonButton.Content>
										<Rectangle Fill="{StaticResource RibbonGroupDialogLauncher}" Width="8" Height="8" />
									</telerikRibbonBar:RadRibbonButton.Content>
								</telerikRibbonBar:RadRibbonButton>

							</Grid>
						</Border>
						<ContentPresenter Margin="4,2,4,18" />
					</Grid>
					

				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	
	

	<Style TargetType="telerikRibbonBar:RadButtonGroup"
           >
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="Background" Value="{StaticResource ButtonGroupBackgroundBrush}" />
		<Setter Property="BorderBrush" Value="{StaticResource ButtonGroupOuterBorderBrush}" />
		<Setter Property="BorderThickness" Value="1" />
