										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="IsOpenState">
								<vsm:VisualState x:Name="Closed" />
								<vsm:VisualState x:Name="Opened">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames Storyboard.TargetName="mouseOverDecorator" Storyboard.TargetProperty="Opacity" Duration="0">
											<DiscreteObjectKeyFrame KeyTime="0" Value="1" />
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames Storyboard.TargetName="SeparatorPart" Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="FocusState">
								<vsm:VisualState x:Name="Unfocused" />
								<vsm:VisualState x:Name="Focused" />
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="Text1VisibilityState">
								<vsm:VisualState x:Name="Text1Show" />
								<vsm:VisualState x:Name="Text1Hide" />
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="Text2VisibilityState">
								<vsm:VisualState x:Name="Text2Show" />
								<vsm:VisualState x:Name="Text2Hide" />
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="DropDownButtonPositionState">
								<vsm:VisualState x:Name="DropDownButtonAtTop" />
								<vsm:VisualState x:Name="DropDownButtonAtLeft" />
								<vsm:VisualState x:Name="DropDownButtonAtBottom" />
								<vsm:VisualState x:Name="DropDownButtonAtRight" />
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="DropDownPlacementState">
								<vsm:VisualState x:Name="PlacementBottom" />
								<vsm:VisualState x:Name="PlacementTop" />
								<vsm:VisualState x:Name="PlacementLeft" />
								<vsm:VisualState x:Name="PlacementRight" />
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="DropDownContentState">
								<vsm:VisualState x:Name="HasDropDownContent" />
								<vsm:VisualState x:Name="WithoutDropDownContent">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
												Storyboard.TargetProperty="Visibility"
												Storyboard.TargetName="DropDownIndicator">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</vsm:VisualState>
							</vsm:VisualStateGroup>
							<vsm:VisualStateGroup x:Name="DropShadowVisibilityState">
								<vsm:VisualState x:Name="HasShadow" />
								<vsm:VisualState x:Name="WithoutShadow" />
							</vsm:VisualStateGroup>
						</vsm:VisualStateManager.VisualStateGroups>
						

						<Border x:Name="backgroundDecorator"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}"
								Background="{x:Null}"
								CornerRadius="1">
							<Border x:Name="backgroundDecoratorInner"
									BorderThickness="{TemplateBinding BorderThickness}"
									BorderBrush="Transparent"
									Background="{TemplateBinding Background}"/>
						</Border>

						<Border x:Name="mouseOverDecorator" Opacity="0"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{StaticResource ApplicationRibbonButtonOverBorder}"
								Background="{x:Null}"
								CornerRadius="1">
							<Border x:Name="mouseOverDecoratorInner"
									BorderThickness="{TemplateBinding BorderThickness}"
									BorderBrush="{StaticResource ApplicationRibbonButtonOverBorderInner}"
									Background="{StaticResource ApplicationRibbonButtonOverBackground}"/>
						</Border>

						<Grid Margin="1">
							<Grid.RowDefinitions>
								<RowDefinition Height="*" />
								<RowDefinition Height="Auto" MaxHeight="28"/>
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="Auto" />
								<ColumnDefinition Width="*" />
								<ColumnDefinition Width="Auto" />
							</Grid.ColumnDefinitions>

							<telerik:RadButton
								x:Name="ButtonPart"
								Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2"
								BorderThickness="0"
								Background="Transparent"
								Content="{TemplateBinding Content}"
								ContentTemplate="{TemplateBinding ContentTemplate}"
								HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
								VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
								Template="{StaticResource AppMenuRadSplitButton_ButtonStyle}"
								Padding="{TemplateBinding Padding}" />
							<telerik:RadToggleButton
								x:Name="DropDownPart"
								Grid.Row="0" Grid.Column="2"
								BorderThickness="0" Margin="0"
								Background="Transparent"
								Visibility="{TemplateBinding DropDownIndicatorVisibility}"
								Template="{StaticResource AppMenuRadSplitButton_DropDownStyle}"
								IsTabStop="false"
								
								IsChecked="{TemplateBinding IsOpen}"
								
								 />
								<Rectangle
								x:Name="SeparatorPart"
								Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2"
								MinWidth="1" MinHeight="1"
								Visibility="Collapsed" IsHitTestVisible="False"
								HorizontalAlignment="Right" VerticalAlignment="Stretch"
								Fill="{StaticResource RadRibbonBarDropDownButtonPressedBorder}" />

								<Image Name="Image" IsHitTestVisible="False" Margin="4"/>

								<TextBlock Name="textRow1" Grid.Row="0" Grid.Column="1" Margin="0,0,2,0" IsHitTestVisible="False"
									Text="{TemplateBinding Text}" TextAlignment="Left"  VerticalAlignment="Center" />

								<Grid Name="DropDownPanel" Grid.Row="0" Grid.Column="2" Margin="0"
									Width="22" IsHitTestVisible="False">
									<Path Name="DropDownIndicator" VerticalAlignment="Center" HorizontalAlignment="Center"
										Data="M 0,0 L 3.5,3.5 L 0,7" Margin="0" IsHitTestVisible="False"
										Visibility="{TemplateBinding DropDownIndicatorVisibility}"
										Fill="{StaticResource RibbonButtonDropDownArrow}"
										Stroke="{StaticResource RibbonButtonDropDownArrowBorder}" />
								</Grid>
						</Grid>
						<Popup
							x:Name="DropDownPopup"
							
							>
							<!--PopupAnimation="{DynamicResource {x:Static SystemParameters.ComboBoxPopupAnimationKey}}">-->
							<Grid>
								<!--<telerikRibbonBarPrimitives:DropShadow telerik:StyleManager.Theme="{StaticResource Theme}" Margin="6,6,4,4" />-->
								<Border x:Name="DropDownPopupBorder"
										BorderBrush="{StaticResource DropDownPopupBorderBrush}"
										Background="{StaticResource DropDownPopupBackgroundBrush}"
										BorderThickness="1"
										MinWidth="3" Width="{TemplateBinding DropDownWidth}"
										MinHeight="3" Height="{TemplateBinding DropDownHeight}">
									<ContentControl
										x:Name="DropDownPopupContent"
										Content="{TemplateBinding DropDownContent}"
										HorizontalAlignment="Stretch"
										VerticalAlignment="Stretch" />
								</Border>
							</Grid>
						</Popup>

						<Rectangle x:Name="disabledDecorator" Visibility="Collapsed" Opacity="0" Fill="#FFFFFFFF" />
					</Grid>

					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

