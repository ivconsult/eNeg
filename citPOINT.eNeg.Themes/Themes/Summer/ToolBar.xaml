                        </vsm:VisualStateManager.VisualStateGroups>
                        

                        <telerikChromes:ButtonChrome
								telerik:StyleManager.Theme="{StaticResource Theme}"
								Background="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Background}"
								BorderBrush="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderBrush}"
								BorderThickness="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness}"
								RenderEnabled="{TemplateBinding IsEnabled}"
								RenderNormal="False"
								RenderActive="{TemplateBinding IsMouseOver}" RenderMouseOver="False"
								RenderPressed="False" RenderChecked="False"
								RenderFocused="{Binding IsFocused, ElementName=ButtonPart}"
								CornerRadius="{TemplateBinding CornerRadius}" />
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <telerik:RadToggleButton x:Name="DropDownPart" IsTabStop="false"
									Margin="0,-1,0,0" Grid.Column="1" Grid.Row="2" MinWidth="11"
									MinHeight="7"
									Visibility="{TemplateBinding DropDownIndicatorVisibility}"
									
                                IsChecked="{TemplateBinding IsOpen}"
                                
                                 >
