			<Path x:Name="RightShapeBackground" Fill="{StaticResource PaneDocumentTabBackground_Normal}" Width="32" Margin="0 0 -32 0" Stretch="None" Data="M0,1.5 C10.5,1.5 12.5,2.5 30,22 L0,22" HorizontalAlignment="Right" />
			<Path x:Name="RightShapeOuterBorder" Stroke="{StaticResource PaneDocumentTabOuterBorder_Normal}" Width="32" Margin="0 0 -32 0" Stretch="None" Data="M0,0.5 C11.5,0.5 13,2 31.5,22" HorizontalAlignment="Right" />
			<Path x:Name="RightShapeInnerBorder" Stroke="{StaticResource PaneDocumentTabInnerBorder_Normal}" Width="32" Margin="0 0 -32 0" Stretch="None" Data="M0,1.5 C10.5,1.5 12.5,2.5 30,22" HorizontalAlignment="Right" />
			
