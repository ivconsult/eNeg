				<RowDefinition Height="*" />
				<RowDefinition Height="auto" />
			</Grid.RowDefinitions>

			<!--TabStrip-->
			<ItemsPresenter x:Name="ItemsPresenterElement" Grid.Row="1"/>

			<!--Content-->
