							<ObjectAnimationUsingKeyFrames Storyboard.TargetName="wrapper" Storyboard.TargetProperty="Margin">
								<DiscreteObjectKeyFrame KeyTime="0" Value="0 0 1 0" />
							</ObjectAnimationUsingKeyFrames>
						</Storyboard>
					</VisualState>
				</VisualStateGroup>
			</VisualStateManager.VisualStateGroups>
			

			<telerikChromes:ButtonChrome x:Name="ChromeUnpinned" Visibility="Collapsed" Style="{StaticResource DocumentTabChromeStyle}" BorderThickness="1" CornerRadius="2" RenderSelected="{TemplateBinding IsSelected}" RenderHighlighted="{TemplateBinding IsMouseOver}" />
