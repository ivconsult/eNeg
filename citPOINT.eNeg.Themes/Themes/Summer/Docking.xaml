	<ControlTemplate x:Key="PaneBottomTemplate" TargetType="telerikDocking:RadPane">
		<Grid x:Name="wrapper" Margin="0 4 1 0">
			
			<VisualStateManager.VisualStateGroups>
				<VisualStateGroup x:Name="CommonStates">
					<VisualState x:Name="Disabled">
						<Storyboard>
							<ObjectAnimationUsingKeyFrames Storyboard.TargetName="HeaderElement" Storyboard.TargetProperty="Opacity">
								<DiscreteObjectKeyFrame KeyTime="0" Value="0.33" />
