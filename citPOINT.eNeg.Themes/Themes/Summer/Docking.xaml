	<Style TargetType="dock:Compass" >
		<Setter Property="Width" Value="105" />
		<Setter Property="Height" Value="105" />
		
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="dock:Compass">
					<Border
						x:Name="VisualRoot"
						Background="{TemplateBinding Background}"
						BorderThickness="{TemplateBinding BorderThickness}"
						BorderBrush="{TemplateBinding BorderBrush}">

						
						<VisualStateManager.VisualStateGroups>
							<VisualStateGroup x:Name="CommonStates">
								<VisualState x:Name="Normal" />
								<VisualState x:Name="HighlightLeftIndicator">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
													Storyboard.TargetName="PART_LeftIndicator"
													Storyboard.TargetProperty="Style">
											<DiscreteObjectKeyFrame KeyTime="0"
														Value="{StaticResource Compass_IndicatorLeftStyle_Highlighted}" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</VisualState>
								<VisualState x:Name="HighlightTopIndicator">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
													Storyboard.TargetName="PART_TopIndicator"
													Storyboard.TargetProperty="Style">
											<DiscreteObjectKeyFrame KeyTime="0"
														Value="{StaticResource Compass_IndicatorTopStyle_Highlighted}" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</VisualState>
								<VisualState x:Name="HighlightRightIndicator">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
													Storyboard.TargetName="PART_RightIndicator"
													Storyboard.TargetProperty="Style">
											<DiscreteObjectKeyFrame KeyTime="0"
														Value="{StaticResource Compass_IndicatorRightStyle_Highlighted}" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</VisualState>
								<VisualState x:Name="HighlightBottomIndicator">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
													Storyboard.TargetName="PART_BottomIndicator"
													Storyboard.TargetProperty="Style">
											<DiscreteObjectKeyFrame KeyTime="0"
														Value="{StaticResource Compass_IndicatorBottomStyle_Highlighted}" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</VisualState>
								<VisualState x:Name="HighlightCenterIndicator">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
													Storyboard.TargetName="PART_CenterIndicator"
													Storyboard.TargetProperty="Style">
											<DiscreteObjectKeyFrame KeyTime="0"
														Value="{StaticResource Compass_IndicatorCenterStyle_Highlighted}" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</VisualState>
							</VisualStateGroup>

							<VisualStateGroup x:Name="LeftIndicatorVisibilityStates">
								<VisualState x:Name="LeftIndicatorVisibile" />
								<VisualState x:Name="LeftIndicatorHidden">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
													Storyboard.TargetName="PART_LeftIndicator"
													Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0"
														Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</VisualState>
							</VisualStateGroup>

							<VisualStateGroup x:Name="TopIndicatorVisibilityStates">
								<VisualState x:Name="TopIndicatorVisibile" />
								<VisualState x:Name="TopIndicatorHidden">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
													Storyboard.TargetName="PART_TopIndicator"
													Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</VisualState>
							</VisualStateGroup>

							<VisualStateGroup x:Name="RightIndicatorVisibilityStates">
								<VisualState x:Name="RightIndicatorVisibile" />
								<VisualState x:Name="RightIndicatorHidden">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
													Storyboard.TargetName="PART_RightIndicator"
													Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</VisualState>
							</VisualStateGroup>

							<VisualStateGroup x:Name="BottomIndicatorVisibilityStates">
								<VisualState x:Name="BottomIndicatorVisibile" />
								<VisualState x:Name="BottomIndicatorHidden">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
													Storyboard.TargetName="PART_BottomIndicator"
													Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</VisualState>
							</VisualStateGroup>

							<VisualStateGroup x:Name="CenterIndicatorVisibilityStates">
								<VisualState x:Name="CenterIndicatorVisibile" />
								<VisualState x:Name="CenterIndicatorHidden">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames
													Storyboard.TargetName="PART_CenterIndicator"
													Storyboard.TargetProperty="Visibility">
											<DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
										</ObjectAnimationUsingKeyFrames>

										<ObjectAnimationUsingKeyFrames
													Storyboard.TargetName="PART_Center"
													Storyboard.TargetProperty="IsHitTestVisible">
											<DiscreteObjectKeyFrame KeyTime="0" Value="False" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</VisualState>
							</VisualStateGroup>
						</VisualStateManager.VisualStateGroups>
						
						
						<Grid
							x:Name="Visual"
							HorizontalAlignment="Center"
							VerticalAlignment="Center">
							
							<ContentControl
								x:Name="PART_Center"
								HorizontalAlignment="Stretch" VerticalAlignment="Stretch"
								Style="{StaticResource Compass_BackgroundStyle}" />
							<ContentControl
								x:Name="PART_LeftIndicator" Margin="4"
								HorizontalAlignment="Left" VerticalAlignment="Center" 
								Style="{StaticResource Compass_IndicatorLeftStyle}"/>
							<ContentControl
								x:Name="PART_TopIndicator" Margin="4"
								HorizontalAlignment="Center" VerticalAlignment="Top"
								Style="{StaticResource Compass_IndicatorTopStyle}"/>
							<ContentControl
								x:Name="PART_RightIndicator" Margin="4"
								HorizontalAlignment="Right" VerticalAlignment="Center" 
								Style="{StaticResource Compass_IndicatorRightStyle}"/>
							<ContentControl
								x:Name="PART_BottomIndicator" Margin="4"
								HorizontalAlignment="Center" VerticalAlignment="Bottom"
								Style="{StaticResource Compass_IndicatorBottomStyle}"/>
							<ContentControl
								x:Name="PART_CenterIndicator"
								HorizontalAlignment="Center" VerticalAlignment="Center"
								Style="{StaticResource Compass_IndicatorCenterStyle}"/>
						</Grid>


					</Border>
					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

