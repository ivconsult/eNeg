							<ObjectAnimationUsingKeyFrames Storyboard.TargetName="BottomBorderSelected" Storyboard.TargetProperty="Visibility">
								<DiscreteObjectKeyFrame KeyTime="0" Value="Visible" />
							</ObjectAnimationUsingKeyFrames>
						</Storyboard>
					</VisualState>
